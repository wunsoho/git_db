sure\\EditDivide-carbon\\EditPower_usage.vue"],"sourcesContent":["<!--탄소 배출 내용 입력의 전력 사용 부분 -->\r\n<template>\r\n    <div>\r\n        <div class=\"edit_top \">\r\n            <span class=\"edit_category\">전력 사용 정보 추가하기</span>\r\n            <button class=\"del_edit_btn\" @click=\"click_del_editPopup()\">x</button>\r\n        </div>\r\n        <div style=\"background:#ffffff; padding:3vh; width:25vw; height:70vh; font-size:1.8vh; overflow:auto\">\r\n            <div>\r\n                <div style=\"color:#000000; \">\r\n                탄소 배출 내용<br>   \r\n                <textarea class=\"addInfo_input\" id=\"carbon_emissions_content\" rows=\"8\" style=\"height:12vh; width:25vw\"></textarea>\r\n                </div> \r\n                <div style=\"margin-top:30px; width:25vw\">기간 설정<br>\r\n                    <input class = \"date_btn\" id = \"start_data\" type=\"month\" data-placeholder=\"시작 날짜\" required aria-required=\"true\" style=\"margin-top:2vh; margin-left:0px; height:3.5vh\">\r\n                    <input class = \"date_btn\" id = \"end_data\" type=\"month\" style=\"margin-left:3vw; height:3.5vh\">\r\n                </div>\r\n                <div style=\"margin-top:30px; font-size:1.8vh\">구분\r\n                    <div class=\"add_info_divide\" id=\"building_name_text\" style=\"margin-top:4vh; width:25vw;\">건물명 / 배출 시설명\r\n                        <input type=\"text\" class=\"addInfo_input\" id =\"building_name_input\" style=\"width:11.5vw; height:3.5vh\" placeholder=\"경상대 본관\">\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">운영주체\r\n                        <select  v-model=\"main_agent\" class=\"addInfo_input\" id=\"operating_entity_input\" style=\"width:12vw; height:4.2vh\">\r\n                            <option value=\"기업\">기업 소유 및 운영</option>\r\n                            <option value=\"민간\">민간 임차</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">공급처\r\n                        <select class=\"addInfo_input\" id=\"supplier_drop\" style=\"width:12vw; height:4.2vh\">\r\n                            <option value=\"0\">한국전력공사</option>\r\n                            <option value=\"1\">직접 입력</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">전력 사용량\r\n                        <input class=\"addInfo_input\" id=\"usage_input\" placeholder=\"12,456\" style=\"width:9vw; height:4vh\">\r\n                        <select v-model=\"unit_s\" class=\"addInfo_input\" id=\"power_usage_drop\" style=\"width:2.5vw; height:4vh\">\r\n                            <option value=\"kwh\">kwh</option>\r\n                            <option value=\"wh\">wh</option>\r\n                        </select>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"edit_bottom\" style=\"font-size:1.5vh\">\r\n            <button class=\"edit_regi_btn\" id=\"edit_regi\" @click=\"click_edit_btn(unit_s)\">수정하기</button>\r\n            <button class=\"edit_regi_btn\" id=\"edit_cancle\" @click=\"click_del_editPopup()\">취소</button>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<style>\r\n    #add_info{\r\n        margin-top: 0px;\r\n        border: 0px;\r\n    }\r\n\r\n    .radio_btn{\r\n        margin-left:60px; \r\n    }\r\n    .addInfo_input{\r\n        width: 80%;\r\n        height: 40px;\r\n        margin-top: 20px;\r\n        background: #F6F7FB;\r\n        border: 0px;\r\n        border-radius: 7px;\r\n    }\r\n    #carbon_emissions_content{\r\n        height:35px;\r\n    }\r\n    .addInfo_input:focus{\r\n        outline: none;\r\n    }\r\n    .add_info_divide{\r\n        margin-top: 20px;\r\n        font-size: 14px;\r\n    }\r\n    .date_btn{\r\n        width: 150px;\r\n        margin-left: 60px;\r\n        height: 40px;\r\n        background: #F6F7FB;\r\n        border: 0px;\r\n        border-radius: 7px;\r\n        color: #727374;\r\n        padding-inline-start: 1%;\r\n        padding-inline-end: 1%;\r\n        content: attr(data-placeholder);\r\n    }\r\n    .date_btn:focus{\r\n        outline:none;\r\n    }\r\n\r\n    #building_name_input{\r\n        margin-top:0px;\r\n        width:20%;\r\n        background: #ffffff;\r\n        border: 1px solid #DDE2E5;\r\n        margin-left: 20px;\r\n    }\r\n\r\n    #operating_entity_input{\r\n        width: 20%;\r\n        margin-left: 95px;\r\n        color: #727374\r\n    }\r\n\r\n    #supplier_drop{\r\n        width: 20%;\r\n        margin-left: 110px;\r\n        color: #727374\r\n    }\r\n    \r\n    #usage_input{\r\n        margin-left:75px;\r\n        width:14%;\r\n        background: #ffffff;\r\n        border: 1px solid #DDE2E5;\r\n    }\r\n\r\n    #power_usage_drop{\r\n        width: 4.6%;\r\n        margin-left: 1%;\r\n        color: #727374;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .del_edit_btn{\r\n        float:right;\r\n        background: none;\r\n        border:none;\r\n        font-size: 25px;\r\n        color:#727374;\r\n        padding:0\r\n    }\r\n    .edit_top{\r\n        background:#ffffff; \r\n        padding:2vh 3vh;\r\n        width:25vw; \r\n        margin-bottom:2px;\r\n    }\r\n    .edit_bottom{\r\n        background:#ffffff; \r\n        padding:2vh 3vh;\r\n        width:25vw; \r\n        margin-bottom:2px;\r\n        float:right;\r\n    }\r\n    .edit_category{\r\n        color:#45464E;\r\n        font-size: 1vw;\r\n        font-weight: 900;\r\n    }\r\n    .edit_regi_btn{\r\n        margin-top:4vh;\r\n        background:#ffffff;\r\n        border: 0.3vh solid #d5d5d5;\r\n        border-radius: 1vh;\r\n        height: 4vh;\r\n        width: 5vw;\r\n        margin-left: 1.8vw;\r\n        font-size: 14px;\r\n    }\r\n    #edit_regi{\r\n        float:right;\r\n        background: #3DC984;\r\n        color:#FFFFFF;\r\n        border: none;\r\n    }\r\n    #edit_cancle{\r\n        float:right;\r\n    }\r\n    \r\n\r\n   \r\n</style>\r\n\r\n<script>\r\nimport {useStore} from 'vuex'\r\nimport {ref,computed} from 'vue'\r\nimport axios from \"axios\";\r\n    export default {\r\n        name :\"power_usage\",\r\n        setup(){\r\n            const store = useStore()\r\n            var unit_s = ref('kwh')\r\n            var main_agent = ref('기업')\r\n            var selected = computed(()=>store.state.selected_row);\r\n            \r\n            async function click_edit_btn(unit_s){\r\n                var info_list={content:\"\",data:\"\",emissions:\"\",StartDate:\"\",EndDate:\"\",scope:\"2\",category:\"7\"}\r\n                var usage_input = document.getElementById('usage_input').value\r\n                info_list.content = document.getElementById('carbon_emissions_content').value\r\n                info_list.data =  usage_input+\"/\"+unit_s\r\n                info_list.emissions = usage_input+4\r\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\r\n                info_list.EndDate = document.getElementById('end_data').value+'-01'\r\n\r\n                if(main_agent.value == '기업'){\r\n                    info_list.scope = '1'\r\n                }\r\n                else if(main_agent.value == \"민간\"){\r\n                    info_list.scope = '2'\r\n                }\r\n                var plz = {\r\n                        \"CarbonData\": {\r\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\r\n                            \"EndDate\":document.getElementById('end_data').value+'-01',\r\n                            \"Location\": \"\",\r\n                            \"Scope\":  Number(info_list.scope),\r\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_content').value,\r\n                            \"CarbonUnit\":unit_s,\r\n                            \"usage\": usage_input+\"/\"+unit_s,\r\n                            \"Chief\": null\r\n                        },\r\n                        \"DetailType\":\"전력\",\r\n                        //\"RootCom\":\"samsung\",\r\n                        //\"BelongCom\":\"\",\r\n                }\r\n\r\n                var table = computed(() => store.state.table_kind)\r\n                console.log(\"테이블 종류\",table.value)\r\n                if(table.value == 'total_table'){\r\n                   //수정 API 연결\r\n                   var config = {\r\n                        headers:{\r\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\r\n                        }\r\n                    }\r\n                   //console.log(selected.value[0])\r\n                   console.log(plz)\r\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\r\n                        console.log(plz)\r\n                        \r\n                    })\r\n                    .catch(error => {\r\n                        alert(\"다시 시도해주세요.\")\r\n                        console.log(error)\r\n                        //router.push('/');\r\n                    })\r\n                    .finally(() => {\r\n                        console.log(\"lender1\")\r\n                    })\r\n                    \r\n                }\r\n                else if(table.value == 'table'){\r\n                    store.commit(\"SetTableContent\",info_list);\r\n                    store.commit('DelTableContent',selected.value);\r\n                }\r\n                store.commit(\"SetEditDelet\");\r\n                location.reload();\r\n            }\r\n            \r\n            function click_del_editPopup(){\r\n                console.log('수정창 닫기')\r\n                store.commit(\"SetEditDelet\");\r\n            }\r\n            return{\r\n                unit_s,\r\n                main_agent,\r\n                selected,\r\n                click_edit_btn,\r\n                click_del_editPopup\r\n            }\r\n        },\r\n    }\r\n    \r\n</script>"],"mappings":";;EAGaA,KAAK,EAAC;AAAW;gCAClBC,mBAAA,CAAgD;EAA1CD,KAAK,EAAC;AAAe,GAAC,eAAa;;EAGxCE,KAAgG,EAAhG;IAAA;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAAgG;;;EAUxFA,KAAwC,EAAxC;IAAA;IAAA;EAAA;AAAwC;gCACzCD,mBAAA,CAEM;EAFDD,KAAK,EAAC,iBAAiB;EAACG,EAAE,EAAC,oBAAoB;EAACD,KAAmC,EAAnC;IAAA;IAAA;EAAA;kCAAoC,eACrF,G,aAAAD,mBAAA,CAA2H;EAApHG,IAAI,EAAC,MAAM;EAACJ,KAAK,EAAC,eAAe;EAACG,EAAE,EAAE,qBAAqB;EAACD,KAAkC,EAAlC;IAAA;IAAA;EAAA,CAAkC;EAACG,WAAW,EAAC;;;EAEjHL,KAAK,EAAC,iBAAiB;EAACE,KAAuB,EAAvB;IAAA;EAAA;;gCAErBD,mBAAA,CAAsC;EAA9BK,KAAK,EAAC;AAAI,GAAC,YAAU;iCAC7BL,mBAAA,CAAiC;EAAzBK,KAAK,EAAC;AAAI,GAAC,OAAK;qBADxBC,UAAsC,EACtCC,WAAiC,C;iCAGzCP,mBAAA,CAKM;EALDD,KAAK,EAAC,iBAAiB;EAACE,KAAuB,EAAvB;IAAA;EAAA;kCAAwB,MACjD,G,aAAAD,mBAAA,CAGS;EAHDD,KAAK,EAAC,eAAe;EAACG,EAAE,EAAC,eAAe;EAACD,KAAgC,EAAhC;IAAA;IAAA;EAAA;iBAC7CD,mBAAA,CAAiC;EAAzBK,KAAK,EAAC;AAAG,GAAC,QAAM,G,aACxBL,mBAAA,CAAgC;EAAxBK,KAAK,EAAC;AAAG,GAAC,OAAK,E;;EAG1BN,KAAK,EAAC,iBAAiB;EAACE,KAAuB,EAAvB;IAAA;EAAA;;iCACzBD,mBAAA,CAAiG;EAA1FD,KAAK,EAAC,eAAe;EAACG,EAAE,EAAC,aAAa;EAACE,WAAW,EAAC,QAAQ;EAACH,KAA6B,EAA7B;IAAA;IAAA;EAAA;;iCAE/DD,mBAAA,CAAgC;EAAxBK,KAAK,EAAC;AAAK,GAAC,KAAG;iCACvBL,mBAAA,CAA8B;EAAtBK,KAAK,EAAC;AAAI,GAAC,IAAE;qBADrBG,WAAgC,EAChCC,WAA8B,C;;EAM7CV,KAAK,EAAC,aAAa;EAACE,KAAuB,EAAvB;IAAA;EAAA;;;uBAzC7BS,mBAAA,CA6CM,cA5CFV,mBAAA,CAGM,OAHNW,UAGM,GAFFC,UAAgD,EAChDZ,mBAAA,CAAsE;IAA9DD,KAAK,EAAC,cAAc;IAAEc,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,MAAA,CAAAC,mBAAmB;KAAI,GAAC,E,GAEjEjB,mBAAA,CAmCM,OAnCNkB,UAmCM,GAlCFlB,mBAAA,CAiCM,cAhCFmB,UAGM,EAKNnB,mBAAA,CAuBM,OAvBNoB,UAuBM,G,iBAvBwC,KAC1C,GAAAC,UAEM,EACNrB,mBAAA,CAKM,OALNsB,UAKM,G,iBAL+C,OACjD,G,gBAAAtB,mBAAA,CAGS;+DAHSgB,MAAA,CAAAO,UAAU,GAAAR,MAAA;IAAEhB,KAAK,EAAC,eAAe;IAACG,EAAE,EAAC,wBAAwB;IAACD,KAAgC,EAAhC;MAAA;MAAA;IAAA;0DAA9De,MAAA,CAAAO,UAAU,E,KAKhCC,WAKM,EACNxB,mBAAA,CAMM,OANNyB,WAMM,G,iBAN+C,SACjD,G