  imgPath\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","name","components","setup","store","account","state","imgPath","require","value","kind","accountInfo","accountNum","money","contentList","id","date","content","check","selected_List","clickContent","index","console","log","i","length","push","splice","deleteContent","editContent","alert","openAddContentPopup","commit"],"sources":["C:\\Users\\user\\DBproject\\git_db\\vueProject\\src\\components\\accountDetail\\AccountDetail.vue"],"sourcesContent":["<template>\r\n    <div style=\"width:40vw;\">\r\n        <!-- 해당 계좌의 총 잔액 -->\r\n        \r\n        <div style=\"margin-top:8vh;\">\r\n            <img class=\"title-bank-icon\" :src=\"imgPath\" style=\"margin-right:6vw; margin-left:3vw\">\r\n            <div id=\"content-total\">\r\n                <div style=\"font-size:2vh\">{{accountInfo.kind}} {{accountInfo.accountNum}}</div>\r\n                <div class=\"title block-title content-block-title\">{{accountInfo.money}}</div>\r\n                <div class=\"title block-title content-block-title\" style=\"margin-left:5vw\">원</div>\r\n            </div>\r\n        </div>\r\n        <!-- 버튼 -->\r\n        <div id = \"content-btn\" style=\"clear:both\">\r\n            <button class=\"btn\" @click=\"deleteContent()\">삭제</button>\r\n            <button class=\"btn\" @click=\"editContent()\">수정</button>\r\n            <input type=\"month\" style=\"border-radius: 0.4vh; background-color: #ffffff; border: 1px solid #A8A8A8; height:2.7vh \">\r\n        </div>\r\n        <div class=\"block content-block\">\r\n            <div v-for=\"content,i in contentList\" >\r\n                <div :class=\"{'block': content.check==false,'click-block':content.check==true}\" id=\"content-block\"  @click=\"clickContent(content,i)\">\r\n                    <div class=\"detail-info\" id=\"content-date\" style=\"\">{{content.date}}</div>\r\n                    <div class=\"detail-info\" id=\"content-content\" style=\"\">{{content.content}}</div>\r\n                    <div v-if=\"content.money > 0\" class=\"detail-info content-money\"  style=\"color:#058B3B; \">+{{content.money}}원</div>\r\n                    <div  v-if=\"content.money == 0\" class=\"detail-info content-money\" style=\"color:#616161(145, 145, 145);\">{{content.money}}원</div>\r\n                    <div  v-if=\"content.money < 0\" class=\"detail-info content-money\" style=\"color:#c80000;\">{{content.money}}원</div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div style=\"width:40vw; height: 10vh;\">\r\n            <div class=\"title add-btn\" @click=\"openAddContentPopup()\">+ 내용 추가하기</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {ref,computed} from 'vue'\r\nimport {useStore} from 'vuex'\r\n\r\n    export default{ \r\n        name:'accountDetailCom',\r\n        components:{\r\n        },\r\n        setup(){\r\n            var store = useStore()\r\n            var account = computed(()=> store.state.account)\r\n            var imgPath = require(\"@/assets/\"+account.value.kind+\".png\")\r\n            var accountInfo = {\r\n                kind:'농협',\r\n                accountNum:'817016-51-003893',\r\n                money:'900,000'\r\n            }\r\n            var contentList = ref([\r\n                {\r\n                    id:1,\r\n                    date:'2023-05-05',\r\n                    content:'초콜릿 구매',\r\n                    money:-2000,\r\n                    id:'',\r\n                    check : false\r\n                },\r\n                {\r\n                    id:2,\r\n                    date:'2023-05-04',\r\n                    content:'초콜릿 구매',\r\n                    money:-2000,\r\n                    id:'',\r\n                    check : false\r\n                },\r\n                {\r\n                    id:3,\r\n                    date:'2023-05-03',\r\n                    content:'용돈',\r\n                    money:300000,\r\n                    id:'',\r\n                    check : false\r\n                },\r\n                {\r\n                    id:4,\r\n                    date:'2023-05-02',\r\n                    content:'알바비',\r\n                    money:600000,\r\n                    id:'',\r\n                    check : false\r\n                },\r\n            ])\r\n\r\n            var selected_List = ref([])\r\n\r\n            function clickContent(content,index){\r\n                console.log(contentList.value[index].check)\r\n                for(var i=0; i<contentList.value.length; i++){\r\n                    if(i==index){\r\n                        if(contentList.value[i].check ==false){\r\n                            contentList.value[i].check = true\r\n                            selected_List.value.push(content)\r\n                        }\r\n                        else{\r\n                            contentList.value[i].check  = false\r\n                            selected_List.value.splice(i,1)\r\n                        }\r\n                        \r\n                    }\r\n                    \r\n                }\r\n                console.log(selected_List.value)\r\n            }\r\n\r\n            function deleteContent(){\r\n      \r\n            }\r\n\r\n            function editContent(){\r\n                if(selected_List.value.length > 1) {\r\n                    alert('하나만 선택하세요')\r\n                }\r\n            }\r\n            function openAddContentPopup(){\r\n                store.commit('openAddContent')\r\n            }\r\n            return{\r\n                contentList,\r\n                accountInfo,\r\n                clickContent,\r\n                selected_List,\r\n                editContent,\r\n                openAddContentPopup,\r\n                imgPath\r\n          \r\n            }\r\n        }\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n#content-total{\r\n    width:40vw; \r\n    height: 12vh;\r\n}\r\n.content-block{\r\n    height:50vh; \r\n    width:38vw; \r\n    margin-top:0; \r\n    padding:1vh 1vw; \r\n    clear:both\r\n}\r\n.title-bank-icon{\r\n    width:5vw;\r\n    height:9.5vh;\r\n    float:left;\r\n}\r\n.content-money{\r\n    width:9vw; \r\n    text-align: right;\r\n}\r\n#content-content{\r\n    margin-left:2vw;\r\n    width:19vw;\r\n}\r\n#content-date{\r\n    color:#616161; \r\n    width:6vw; \r\n    margin-left:1vw\r\n}\r\n.detail-info{\r\n    display:inline-block;\r\n    line-height: 5vh;\r\n    height: 5vh; \r\n    font-size:1.8vh;\r\n    font-weight: 600;\r\n}\r\n.btn{\r\n    float:right;\r\n    width:3.5vw;\r\n    height:3vh;\r\n    margin-left:0.5vw;\r\n    background-color:white;\r\n    border:1px solid #A8A8A8;\r\n    border-radius: 0.6vh;\r\n\r\n}\r\n.content-block-title{\r\n    line-height:2vh\r\n}\r\n.btn:hover{\r\n    cursor: pointer;\r\n    background-color:#e1e1e1\r\n}\r\n.add-btn{\r\n    color:#146390\r\n}\r\n.add-btn:hover{\r\n    cursor: pointer;\r\n}\r\n#content-block{\r\n    height:5vh; \r\n    width:38vw; \r\n    margin-top:1vh; \r\n    border:1px solid #A8A8A8;\r\n    border-radius: 1vh;\r\n}\r\n.click-block{\r\n    background-color:#f5f6f6;\r\n}\r\n#content-btn{\r\n    width:40vw; \r\n    margin-top:4vh; \r\n    height: 4vh;\r\n}\r\n</style>"],"mappings":";AAoCA,SAAQA,GAAG,EAACC,QAAQ,QAAO,KAAI;AAC/B,SAAQC,QAAQ,QAAO,MAAK;AAExB,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,KAAI,GAAIJ,QAAQ,EAAC;IACrB,IAAIK,OAAM,GAAIN,QAAQ,CAAC,MAAKK,KAAK,CAACE,KAAK,CAACD,OAAO;IAC/C,IAAIE,OAAM,GAAIC,OAAO,CAAC,WAAW,GAACH,OAAO,CAACI,KAAK,CAACC,IAAI,GAAC,MAAM;IAC3D,IAAIC,WAAU,GAAI;MACdD,IAAI,EAAC,IAAI;MACTE,UAAU,EAAC,kBAAkB;MAC7BC,KAAK,EAAC;IACV;IACA,IAAIC,WAAU,GAAIhB,GAAG,CAAC,CAClB;MACIiB,EAAE,EAAC,CAAC;MACJC,IAAI,EAAC,YAAY;MACjBC,OAAO,EAAC,QAAQ;MAChBJ,KAAK,EAAC,CAAC,IAAI;MACXE,EAAE,EAAC,EAAE;MACLG,KAAI,EAAI;IACZ,CAAC,EACD;MACIH,EAAE,EAAC,CAAC;MACJC,IAAI,EAAC,YAAY;MACjBC,OAAO,EAAC,QAAQ;MAChBJ,KAAK,EAAC,CAAC