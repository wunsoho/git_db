ssion/\" + selected.value[0].id, plz, config).then(res => {\n          console.log(plz);\n        }).catch(error => {\n          alert(\"다시 시도해주세요.\");\n          console.log(error);\n          //router.push('/');\n        }).finally(() => {\n          console.log(\"lender1\");\n        });\n      } else if (table.value == 'table') {\n        store.commit(\"SetTableContent\", info_list);\n        store.commit('DelTableContent', selected.value);\n      }\n      store.commit(\"SetEditDelet\");\n      location.reload();\n    }\n    function click_del_editPopup() {\n      console.log('수정창 닫기');\n      store.commit(\"SetEditDelet\");\n    }\n    return {\n      click_edit_btn,\n      click_del_editPopup\n    };\n  }\n};","map":{"version":3,"names":["useStore","ref","computed","axios","name","setup","selected","store","state","selected_row","click_edit_btn","info_list","content","data","emissions","StartDate","EndDate","scope","category","usage_input","document","getElementById","value","plz","Number","unit_s","table","table_kind","console","log","config","headers","accessToken","put","id","then","res","catch","error","alert","finally","commit","location","reload","click_del_editPopup"],"sources":["/home/hyemi/carbon/co2-emission-management/vueProject/src/components/measure/EditDivide-carbon/EditWaste_disposal(Wastewater).vue"],"sourcesContent":["<!--폐기물 처리시설 (폐수 처리)  -->\n<template>\n    <div>\n        <div class=\"edit_top \">\n            <span class=\"edit_category\">폐기물 처리시설(폐수처리) 정보 추가하기</span>\n            <button class=\"del_edit_btn\" @click=\"click_del_editPopup()\">x</button>\n        </div>\n        <div style=\"background:#ffffff; padding:3vh; width:25vw; height:70vh; font-size:1.8vh\">\n            <div>\n                <div style=\"color:#000000; \">\n                탄소 배출 내용<br>   \n                <textarea class=\"addInfo_input\" id=\"carbon_emissions_conten_wasteWater\" rows=\"8\" style=\"height:12vh; width:25vw\"></textarea>\n                </div> \n                <div style=\"margin-top:30px; width:25vw\">하폐수 처리 날짜<br>\n                    <input class = \"date_btn\" id = \"start_data\" type=\"month\" data-placeholder=\"시작 날짜\" required aria-required=\"true\" style=\"margin-top:2vh; margin-left:0px; height:3.5vh\">\n                    <input class = \"date_btn\" id = \"end_data\" type=\"month\" style=\"margin-left:3vw; height:3.5vh\">\n                </div>\n                <div class=\"add_info_divide\" style=\"float:left; width:600px;\">폐수 처리량\n                    <input class=\"addInfo_input\" id=\"steam_usage_input\" placeholder=\"12,456\" style=\"width:8.5vw; margin-left:3vw;\">\n                    <select class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"width:3vw; margin-left:12px;\">\n                        <option value=\"0\">m^3/1일</option>\n                    </select>\n                </div>\n                <div class=\"add_info_divide\" style=\"width:600px;\" >메탄 회수량\n                    <input class=\"addInfo_input\" id=\"steam_usage_input\" style=\"width:8.5vw; margin-left:3vw\" placeholder=\"12,456\">\n                    <select class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"width:3vw; margin-left:12px;\">\n                        <option value=\"0\">ton</option>\n                    </select>\n                </div>\n                <div class=\"add_info_divide\" style=\" width:600px;\" >유입 농도(COD)\n                    <input class=\"addInfo_input\" id=\"steam_usage_input\" style=\"width:8.5vw; margin-left:1.7vw\" placeholder=\"12,456\">\n                    <select class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"width:3vw; margin-left:12px;\">\n                        <option value=\"0\">mg/L</option>\n                    </select>\n                </div>\n                <div class=\"add_info_divide\" style=\"margin-top:0\">유출 농도(COD)\n                    <input class=\"addInfo_input\" id=\"steam_usage_input\" placeholder=\"12,456\" style=\" width:8.5vw; margin-left:1.7vw\">\n                    <select class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"width:3vw; margin-left:12px;\">\n                        <option value=\"0\">mg/L</option>\n                    </select>\n                </div>\n            </div>\n        </div>\n        <div class=\"edit_bottom\" style=\"font-size:1.5vh\">\n            <button class=\"edit_regi_btn\" id=\"edit_regi\" @click=\"click_edit_btn()\">수정하기</button>\n            <button class=\"edit_regi_btn\" id=\"edit_cancle\" @click=\"click_del_editPopup()\">취소</button>\n        </div>\n    </div>\n\n</template>\n\n<style>\n\n    #supplier_drop_steam{\n        width: 20%;\n        margin-left: 90px;\n        color: #727374\n    }\n\n    #steam_usage_input{\n        margin-left:90px;\n        width:14%;\n        background: #ffffff;\n        border: 1px solid #DDE2E5;\n    }\n\n    #steam_usage_drop{\n        width: 4.6%;\n        margin-left: 1%;\n        color: #727374;\n        margin-bottom: 20px;\n    }\n\n   \n</style>\n\n<script>\nimport {useStore} from 'vuex'\nimport {ref,computed} from 'vue'\nimport axios from \"axios\";\n    export default {\n        name :\"waste_disposal_Wastewater\",\n        setup(){\n            var selected = computed(()=>store.state.selected_row);\n            const store = useStore()\n            async function click_edit_btn(){\n                var info_list = {content:\"\",data:\"\",emissions:\"\",StartDate:\"\",EndDate:\"\",scope:\"1\",category:\"18\"}\n                var usage_input = document.getElementById('steam_usage_input').value\n                info_list.content = document.getElementById('carbon_emissions_conten_wasteWater').value\n                info_list.data =  usage_input+\"/\"+\"ton\"\n                info_list.emissions = usage_input+4\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\n                info_list.EndDate = document.getElementById('end_data').value+'-01'\n\n                var plz = {\n                        \"CarbonData\": {\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\n                            \"EndDate\":document.getElementById('end_data').value+'-01',\n                            \"Location\": \"\",\n                            \"Scope\":  Number(info_list.scope),\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_conten_wasteWater').value,\n                            \"CarbonUnit\": unit_s,\n                            \"usage\": usage_input+\"/\"+unit_s,\n                            \"Chief\": null\n                        },\n                        \"DetailType\":\"폐기물 처리시설\",\n                        //\"RootCom\":\"samsung\",\n                        //\"BelongCom\":\"\",\n                }\n                var table = computed(() => store.state.table_kind)\n                console.log(\"테이블 종류\",table)\n                if(table.value == 'total_table'){\n                    //수정 API 연결\n                    var config = {\n                        headers:{\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\n                        }\n                    }\n                   //console.log(selected.value[0])\n                   console.log(plz)\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\n                        console.log(plz)\n                        \n                    })\n                    .catch(error => {\n                        alert(\"다시 시도해주세요.\")\n                        console.log(error)\n                        //router.push('/');\n                    })\n                    .finally(() => {\n                        console.log(\"lender1\")\n                    })\n                }\n                else if(table.value == 'table'){\n                    store.commit(\"SetTableContent\",info_list);\n                    store.commit('DelTableContent',selected.value);\n                }\n                store.commit(\"SetEditDelet\");\n                location.reload();\n            }\n            \n            function click_del_editPopup(){\n                console.log('수정창 닫기')\n                store.commit(\"SetEditDelet\");\n            }\n            return{\n                click_edit_btn,\n                click_del_editPopup\n            }\n        }\n    }\n</script>"],"mappings":"AA6EA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,GAAG,EAACC,QAAQ,QAAO,KAAI;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,2BAA2B;EACjCC,KAAKA,CAAA,EAAE;IACH,IAAIC,QAAO,GAAIJ,QAAQ,CAAC,MAAIK,KAAK,CAACC,KAAK,CAACC,YAAY,CAAC;IACrD,MAAMF,KAAI,GAAIP,QAAQ,EAAC;IACvB,eAAeU,cAAcA,CAAA,EAAE;MAC3B,IAAIC,SAAQ,GAAI;QAACC,OAAO,EAAC,EAAE;QAACC,IAAI,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,KAAK,EAAC,GAAG;QAACC,QAAQ,EAAC;MAAI;MAChG,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,mBAAmB,CAAC,CAACC,KAAI;MACnEX,SAAS,CAACC,OAAM,GAAIQ,QAAQ,CAACC,cAAc,CAAC,oCAAoC,CAAC,CAACC,KAAI;MACtFX,SAAS,CAACE,IAAG,GAAKM,WAAW,GAAC,GAAG,GAAC,KAAI;MACtCR,SAAS,CAACG,SAAQ,GAAIK,WAAW,GAAC;MAClCR,SAAS,CAACI,SAAQ,GAAIK,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtEX,SAAS,CAACK,OAAM,GAAII,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAI;MAElE,IAAIC,GAAE,GAAI;QACF,YAAY,EAAE;UACV,WAAW,EAACH,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC7D,SAAS,EAACF,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAK;UACzD,UAAU,EAAE,EAAE;UACd,OAAO,EAAGE,MAAM,CAACb,SAAS,CAACM,KAAK,CAAC;UACjC,gBAAgB,EAAEG,QAAQ,CAACC,cAAc,CAAC,oCAAoC,CAAC,CAACC,KAAK;UACrF,YAAY,EAAEG,MAAM;UACpB,OAAO,EAAEN,WAAW,GAAC,GAAG,GAACM,MAAM;UAC/B,OAAO,EAAE;QACb,CAAC;QACD,YAAY,EAAC;QACb;QACA;MACR;;MACA,IAAIC,KAAI,GAAIxB,QAAQ,CAAC,MAAMK,KAAK,CAACC,KAAK,CAACmB,UAAU;MACjDC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAACH,KAAK;MAC1B,IAAGA,KAAK,CAACJ,KAAI,IAAK,aAAa,EAAC;QAC5B;QACA,IAAIQ,MAAK,GAAI;UACTC,OAAO,EAAC;YACJ,eAAe,EAAC,QAAQ,GAAC,GAAG,GAACxB,KAAK,CAACC,KAAK,CAACwB;UA