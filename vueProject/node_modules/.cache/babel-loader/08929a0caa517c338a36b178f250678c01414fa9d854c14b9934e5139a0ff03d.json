{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport income from '@/components/patternAnalysis/income';\nimport expend from '@/components/patternAnalysis/expend';\nimport axios from 'axios';\nexport default {\n  name: 'accountDetailCom',\n  components: {\n    income,\n    expend\n  },\n  setup() {\n    var expendList = [{\n      kind: \"여가\",\n      money: 20186\n    }, {\n      kind: \"간식\",\n      money: 80000\n    }, {\n      kind: \"생필품\",\n      money: 20186\n    }, {\n      kind: \"식비\",\n      money: 5802\n    }, {\n      kind: \"음식점\",\n      money: 6050\n    }];\n    var incomList = [{\n      kind: \"월급\",\n      money: 806000\n    }, {\n      kind: \"용돈\",\n      money: 350000\n    }];\n    var activeColor = ['#FF7E66', '#FFBD60', '#A7B901', '#00A29B', '#EEE8AC', '#E9DF56'];\n    var store = useStore();\n    function clickPattern() {\n      store.commit(\"clickPatternPopup\");\n    }\n    var pattern = computed(() => store.state.pattern);\n    var month = ref(computed(() => store.state.month));\n    var date = ref('2023-' + month.value);\n    console.log(month.value);\n    function monthCal() {\n      console.log(date.value);\n      store.commit(\"setMonth\", date.value.substr(5, 2));\n    }\n    async function get_expend_pattern() {\n      await axios.get(\"/api/users/getExpendPattern/\" + month.value).then(res => {\n        console.log('카테고리별 총 금액', res.data);\n        var expend_label = [];\n        var expend_data = [];\n        for (var i = 0; i < res.data.length; i++) {\n          expend_label.push(res.data[i].ex_category);\n          expend_data.push(res.data[i].money);\n        }\n        console.log(expend_label);\n        console.log(expend_data);\n        store.commit('setExpendPattern', expend_label, expend_data);\n      }).catch(error => {\n        console.log(error);\n      }).finally(() => {});\n    }\n    async function get_income_pattern() {\n      await axios.get(\"/api/users/getIncomePattern/\" + month.value).then(res => {\n        console.log('카테고리별 총 금액', res.data);\n        var income_label = [];\n        var income_data = [];\n        for (var i = 0; i < res.data.length; i++) {\n          income_label.value.push(res.data[i].in_category);\n          income_data.value.push(res.data[i].money);\n        }\n        console.log(income_label);\n        console.log(income_data);\n        store.commit('setIncomePattern', income_label, income_data);\n      }).catch(error => {\n        console.log(error);\n      }).finally(() => {});\n    }\n    get_expend_pattern();\n    get_income_pattern();\n    return {\n      activeColor,\n      expendList,\n      incomList,\n      clickPattern,\n      pattern,\n      date,\n      monthCal,\n      month\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","income","expend","axios","name","components","setup","expendList","kind","money","incomList","activeColor","store","clickPattern","commit","pattern","state","month","date","value","console","log","monthCal","substr","get_expend_pattern","get","then","res","data","expend_label","expend_data","i","length","push","ex_category","catch","error","finally","get_income_pattern","income_label","income_data","in_category"],"sources":["C:\\Users\\gpal6\\DBproject\\git_db\\vueProject\\src\\components\\patternAnalysis\\patternPopup.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div style=\"width:3vw; height:18vh; background-color: #D9D9D9; float:right; border-radius: 0 4vh 4vh 0;\" @click =\"clickPattern()\">\r\n            <div style=\"width:inherit; text-align: center; margin-top:2vh; font-size:1.6vh; font-weight: bold; \" >\r\n                패 <br>\r\n                턴 <br>\r\n                분 <br>\r\n                석\r\n            </div>\r\n        </div>\r\n        <div class=\"pattern-block\" style=\"float:left\" v-if=\"pattern==true\">\r\n            <div class=\"pattern-header\">\r\n                <input type=\"month\" id=\"currnetMonth\" v-model=\"date\" @change=\"monthCal()\">\r\n                <div>수입, 지출 패턴 분석</div>\r\n            </div>\r\n\r\n            <div style=\"margin-top:4vh\">\r\n                <div class=\"pattern-title\">수입 패턴 분석</div>\r\n                <div class=\"pattern-board\">\r\n                    <income style=\"height:20vh; width:15vw; float:right\"></income>\r\n                    <div style=\"float:left; margin-left:2vw; overflow: auto; height: 20vh; padding:0 1vw\">\r\n                        <div v-for=\"incom, i in incomList\" >\r\n                            <div class=\"legend-style\" :style=\"{ background:activeColor[i]}\"></div>\r\n                            <span style=\"display:inline-block; width:5vw; margin-bottom:1.7vh\">{{incom.kind}}</span><span>{{ incom.money }}원</span>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n   \r\n            <div style=\"margin-top:4vh\">\r\n                <div class=\"pattern-title\">지출 패턴 분석</div>\r\n                <div class=\"pattern-board\">\r\n                    <expend style=\"height:20vh; width:15vw; float:left\"></expend>\r\n                    <div style=\"float:left; margin-left:2vw; overflow: auto; height: 20vh; padding:0 1vw\">\r\n                        <div v-for=\"expend, i in expendList\" >\r\n                            <div class=\"legend-style\" :style=\"{ background:activeColor[i]}\"></div>\r\n                            <span style=\"display:inline-block; width:5vw; margin-bottom:1.7vh\">{{expend.kind}}</span><span>{{ expend.money }}원</span>\r\n                        </div>\r\n                    </div>\r\n                    \r\n                </div>\r\n            </div>\r\n\r\n        </div> \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {ref,computed} from 'vue'\r\nimport { useStore } from 'vuex';\r\nimport income from '@/components/patternAnalysis/income'\r\nimport expend from '@/components/patternAnalysis/expend'\r\nimport axios from 'axios'\r\n    export default{ \r\n        name:'accountDetailCom',\r\n        components:{\r\n            income,\r\n            expend\r\n        },\r\n        setup(){\r\n            var expendList =[\r\n                {kind:\"여가\", money: 20186},\r\n                {kind:\"간식\", money: 80000},\r\n                {kind:\"생필품\", money: 20186},\r\n                {kind:\"식비\", money: 5802},\r\n                {kind:\"음식점\", money: 6050}\r\n            ]\r\n\r\n            var incomList = [\r\n                {kind:\"월급\", money:806000},\r\n                {kind:\"용돈\", money:350000}\r\n            ]\r\n            \r\n            var activeColor = ['#FF7E66','#FFBD60','#A7B901','#00A29B','#EEE8AC','#E9DF56']\r\n            var store = useStore();\r\n            function clickPattern(){\r\n                store.commit(\"clickPatternPopup\")\r\n            }\r\n            var pattern = computed(()=>store.state.pattern)\r\n            var month =  ref(computed(()=>store.state.month))\r\n            var date = ref('2023-'+ month.value)\r\n\r\n            console.log(month.value)\r\n            function monthCal(){\r\n                console.log(date.value)\r\n                store.commit(\"setMonth\",date.value.substr(5, 2))\r\n            }\r\n\r\n            async function get_expend_pattern(){\r\n                await axios.get(\"/api/users/getExpendPattern/\"+month.value).then(res => {\r\n                    console.log('카테고리별 총 금액',res.data)\r\n                    var expend_label =[]\r\n                    var expend_data = []\r\n                    for (var i =0; i<res.data.length; i++){\r\n                        expend_label.push(res.data[i].ex_category)\r\n                        expend_data.push(res.data[i].money)\r\n                    }\r\n\r\n                    console.log(expend_label)\r\n                    console.log(expend_data)\r\n                    store.commit('setExpendPattern',expend_label,expend_data)\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n                .finally(() => {\r\n                })\r\n            }\r\n            \r\n            async function get_income_pattern(){\r\n                await axios.get(\"/api/users/getIncomePattern/\"+month.value).then(res => {\r\n                    console.log('카테고리별 총 금액',res.data)\r\n                    var income_label =[]\r\n                    var income_data = []\r\n                    for (var i =0; i<res.data.length; i++){\r\n                        income_label.value.push(res.data[i].in_category)\r\n                        income_data.value.push(res.data[i].money)\r\n                    }\r\n\r\n                    console.log(income_label)\r\n                    console.log(income_data)\r\n                    store.commit('setIncomePattern',income_label,income_data)\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                })\r\n                .finally(() => {\r\n                })\r\n            }\r\n            get_expend_pattern()\r\n            get_income_pattern()\r\n            return{\r\n                activeColor,\r\n                expendList,\r\n                incomList,\r\n                clickPattern,\r\n                pattern,\r\n                date,\r\n                monthCal,\r\n                month\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n.pattern-block{\r\n    width:32vw;\r\n    height:100vh;\r\n    background: #D9D9D9;\r\n    /* margin-left:65vw; */\r\n    padding:2vh 1.5vw;\r\n}\r\n.pattern-header{\r\n    margin-left:10vw;\r\n}\r\n.pattern-header > input {\r\n    width:12vw;\r\n    height:5vh;\r\n    background : none;\r\n    border: none;\r\n    font-size: 3vh;\r\n    font-weight: bold;\r\n}\r\n\r\n.pattern-header > div {\r\n    width:12vw;\r\n    height:5vh;\r\n    margin-top:2vh;\r\n    text-align: center;\r\n    background : none;\r\n    border: none;\r\n    font-size: 2vh;\r\n    font-weight: bold;\r\n}\r\n.pattern-title{\r\n    font-size:1.7vh;\r\n    font-weight: bold;\r\n    color: #002641\r\n}\r\n.pattern-board{\r\n    margin-top:1vh;\r\n    width:inherit;\r\n    height: 20vh;\r\n    background: #ffffff;\r\n    border-radius: 2vh;\r\n    padding:6vh 1vw;\r\n}\r\n.legend-style{\r\n    display:inline-block;\r\n    height:1.5vh;\r\n    width:1.5vh;\r\n    border-radius: 50%;\r\n    background: #5A5A5A;\r\n    margin-right:1vw;\r\n    \r\n}\r\n</style>"],"mappings":";AAgDA,SAAQA,GAAG,EAACC,QAAQ,QAAO,KAAI;AAC/B,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,MAAK,MAAO,qCAAoC;AACvD,OAAOC,MAAK,MAAO,qCAAoC;AACvD,OAAOC,KAAI,MAAO,OAAM;AACpB,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC;IACPJ,MAAM;IACNC;EACJ,CAAC;EACDI,KAAKA,CAAA,EAAE;IACH,IAAIC,UAAS,GAAG,CACZ;MAACC,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAE;IAAK,CAAC,EACzB;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAE;IAAK,CAAC,EACzB;MAACD,IAAI,EAAC,KAAK;MAAEC,KAAK,EAAE;IAAK,CAAC,EAC1B;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAE;IAAI,CAAC,EACxB;MAACD,IAAI,EAAC,KAAK;MAAEC,KAAK,EAAE;IAAI,EAC5B;IAEA,IAAIC,SAAQ,GAAI,CACZ;MAACF,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAC;IAAM,CAAC,EACzB;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAC;IAAM,EAC5B;IAEA,IAAIE,WAAU,GAAI,CAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS;IAC9E,IAAIC,KAAI,GAAIZ,QAAQ,EAAE;IACtB,SAASa,YAAYA,CAAA,EAAE;MACnBD,KAAK,CAACE,MAAM,CAAC,mBAAmB;IACpC;IACA,IAAIC,OAAM,GAAIhB,QAAQ,CAAC,MAAIa,KAAK,CAACI,KAAK,CAACD,OAAO;IAC9C,IAAIE,KAAI,GAAKnB,GAAG,CAACC,QAAQ,CAAC,MAAIa,KAAK,CAACI,KAAK,CAACC,KAAK,CAAC;IAChD,IAAIC,IAAG,GAAIpB,GAAG,CAAC,OAAO,GAAEmB,KAAK,CAACE,KAAK;IAEnCC,OAAO,CAACC,GAAG,CAACJ,KAAK,CAACE,KAAK;IACvB,SAASG,QAAQA,CAAA,EAAE;MACfF,OAAO,CAACC,GAAG,CAACH,IAAI,CAACC,KAAK;MACtBP,KAAK,CAACE,MAAM,CAAC,UAAU,EAACI,IAAI,CAACC,KAAK,CAACI,MAAM,CAAC,CAAC,EAAE,CAAC,CAAC;IACnD;IAEA,eAAeC,kBAAkBA,CAAA,EAAE;MAC/B,MAAMrB,KAAK,CAACsB,GAAG,CAAC,8BAA8B,GAACR,KAAK,CAACE,KAAK,CAAC,CAACO,IAAI,CAACC,GAAE,