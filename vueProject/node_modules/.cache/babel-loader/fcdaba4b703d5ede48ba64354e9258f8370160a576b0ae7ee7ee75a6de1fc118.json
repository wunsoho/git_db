MemberOut = () => {\n      point_member.value.select = null;\n      point_member.value.point = false;\n    };\n    async function member_get_list() {\n      console.log(\"dawdaw\");\n      await axios.get('/member').then(res => {\n        this.memberInfo = res.data;\n        console.log(this.memberInfo);\n      }).catch(error => {\n        console.log(error);\n      }).finally(() => {});\n    }\n    console.log('member', memberInfo.value);\n    return {\n      memberInfo,\n      position,\n      point_member,\n      pointMember,\n      pointMemberOut,\n      openAddMemberPop,\n      member_get_list\n    };\n  },\n  created() {\n    this.member_get_list();\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","axios","name","components","setup","store","openAddMemberPop","commit","point_member","select","point","position","index","testmemberInfo","Name","phoneNum","email","gitLink","homepageLink","memberInfo","pointMember","value","pointMemberOut","member_get_list","console","log","get","then","res","data","catch","error","finally","created"],"sources":["/home/hyemi/bigsysWeb/wp-bigsys/src/vueProject/src/components/member/PageMember.vue"],"sourcesContent":["<template>\n    <div class=\"page-content\" id=\"member-page-content\">\n        <div style=\"margin-bottom:18vh\" v-for=\"position in position\">\n            <div class=\"page-text\">{{ position.position }}\n                <button v-if =\"position.position!='Professor'\" class=\"btn_\"><img src=\"@/assets/user_add.png\" ></button>\n                <div class=\"line\"></div>\n            </div>\n            <div v-for=\"member in memberInfo\" >\n                <div v-if=\"position.index == member.Position\" class=\"member\" v-on:mouseover=\"pointMember(member.index)\" v-on:mouseleave=\"pointMemberOut()\">\n                    <img class=\"member-img\" src=\"@/assets/defalutPeople.png\">\n                    <span style=\"float: left; margin-left:2vw\">\n                        <div style=\"font-size:1.5rem; font-weight:bold; height: 8vh; width:29vw; \">{{member.Name}}</div>\n                        <div style=\"font-size:1.3rem; height:4vh; line-height: 4vh; width:29vw;\">tell :  {{ member.PhoneNum }}</div>\n                        <div style=\"font-size:1.3rem; height:4vh; line-height: 4vh; width:29vw;\">email :  {{ member.Email }}</div>\n                        <div style=\"font-size:1.3rem; height:4vh; line-height: 4vh; width:29vw; display: inline-block;\" v-if=\"member.HomePage!=null\">homepageLink :  {{ member.HomePage }}</div>\n                        <a :href=\"member.Git\" style=\"margin-left:2vw; display: inline-block; float:right\"><img style=\"height:4vh\" src=\"@/assets/gitIcon.png\"></a>\n                    </span>            \n                </div>  \n                <img class =\"small-btn\" v-if=\" point_member.point == true && point_member.select == member.index && member.index!=1\" src=\"@/assets/delete.png\" @click=\"openAddMemberPop()\">\n            </div>\n        </div>\n        \n    </div>\n</template>\n\n\n\n<script>\nimport { ref,computed } from \"vue\"\nimport { useStore } from \"vuex\"\nimport axios from 'axios';\n\n    export default {\n        name :\"pageMember\",\n        components:{\n           \n        },\n        setup(){\n            const store = useStore()\n\n            const openAddMemberPop = () => store.commit(\"openAddMember\");\n            var point_member =ref({select:'', point: false})\n            var position =[\n                {position:'Professor', index:0},\n                {position:'Doctor Course', index:1},\n                {position:'Master Course', index:2},\n                {position:'Undergraduate', index:3},\n            ]\n            var testmemberInfo = [\n                {index:1 , position: 'Professor', Name:'김재호', phoneNum:'010-0000-0000', email:'gggg@djdjdjd', gitLink:'',homepageLink:''},\n                {index:2 ,position: 'Doctor Course', Name:'김재호', phoneNum:'010-0000-0000', email:'gggg@djdjdjd' , gitLink:'',homepageLink:''},\n                {index:3 ,position: 'Master Course', Name:'김재호', phoneNum:'010-0000-0000', email:'gggg@djdjdjd' , gitLink:'',homepageLink:''},\n                {index:4 ,position: 'Undergraduate', name:'정혜미', phoneNum:'010-9606-5072', email:'gpal6072@naver.com' , gitLink:'https://github.com/hyemijeong',homepageLink:''},\n                {index:5 ,position: 'Undergraduate', Name:'김재호', phoneNum:'010-0000-0000', email:'gggg@djdjdjd' , gitLink:'',homepageLink:''},\n                {index:6 ,position: 'Undergraduate', name:'김재호', phoneNum:'010-0000-0000', email:'gggg@djdjdjd', gitLink:'' ,homepageLink:''},\n            ]\n            var memberInfo = ref([])\n            \n            const pointMember = (index) => {\n                point_member.value.select = index\n                point_member.value.point = true\n            } \n            const pointMemberOut = () => {\n                point_member.value.select = null\n                point_member.value.point = false\n            }\n            async function member_get_list(){\n                console.log(\"dawdaw\")\n                await axios.get('/member').then(res => {\n                    this.memberInfo =  res.data\n                    console.log(this.memberInfo)\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n                .finally(() => {})\n            }\n            console.log('member',memberInfo.value)\n\n            return{   \n                memberInfo,\n                position,\n                point_member,\n                pointMember,\n                pointMemberOut,\n                openAddMemberPop,\n                member_get_list,\n            }\n        },\n        created(){\n            this.member_get_list()\n        }\n    }\n</script>\n\n\n<style>\n.page-content{\n    padding: 20vh 10vw;\n}\n#member-page-content{\n    padding: 20vh 25vw;\n}\n.page-text{\n    font-size:2rem;\n    font-weight: bold;\n}\n.line{\n    width:inherit; \n    height:1px; \n    background: black; \n    margin-top:1vh;\n}\n.member{\n    padding:2%;\n    height: 22vh; \n    margin-top:5vh;\n}\n.member:hover{\n    border: 1px solid #d1d1d1;\n    border-radius: 1vh;\n}\n.member-img{\n    float:left;\n    background: #d5d5d5;\n    height: 12vh;\n    padding:5vh 3vh;\n}\n.btn_ img{\n    height: 4vh;\n}\n.btn_{\n    background: none;\n    border: none;\n    height: 4vh;\n    float : right\n}\n\n.btn_:hover{\n    cursor: pointer;\n}\n.small-btn{\n    height:2vh;\n    float: right;\n}\n.small-btn:hover{\n    cursor: pointer;\n}\n</style>"],"mappings":"AA4BA,SAASA,GAAG,EAACC,QAAO,QAAS,KAAI;AACjC,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,KAAI,MAAO,OAAO;AAErB,eAAe;EACXC,IAAG,EAAG,YAAY;EAClBC,UAAU,EAAC,CAEX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IAEvB,MAAMM,gBAAe,GAAIA,CAAA,KAAMD,KAAK,CAACE,MAAM,CAAC,eAAe,CAAC;IAC5D,IAAIC,YAAW,GAAGV,GAAG,CAAC;MAACW,MAAM,EAAC,EAAE;MAAEC,KAAK,EAAE;IAAK,CAAC;IAC/C,IAAIC,QAAO,GAAG,CACV;MAACA,QAAQ,EAAC,WAAW;MAAEC,KAAK,EAAC;IAAC,CAAC,EAC/B;MAACD,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC;IAAC,CAAC,EACnC;MAACD,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC;IAAC,CAAC,EACnC;MAACD,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC;IAAC,CAAC,CACvC;IACA,IAAIC,cAAa,GAAI,CACjB;MAACD,KAAK,EAAC;MAAID,QAAQ,EAAE,WAAW;MAAEG,IAAI,EAAC,KAAK;MAAEC,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC,cAAc;MAAEC,OAAO,EAAC,EAAE;MAACC,YAAY,EAAC;IAAE,CAAC,EACzH;MAACN,KAAK,EAAC;MAAGD,QAAQ,EAAE,eAAe;MAAEG,IAAI,EAAC,KAAK;MAAEC,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC,cAAa;MAAIC,OAAO,EAAC,EAAE;MAACC,YAAY,EAAC;IAAE,CAAC,EAC7H;MAACN,KAAK,EAAC;MAAGD,QAAQ,EAAE,eAAe;MAAEG,IAAI,EAAC,KAAK;MAAEC,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC,cAAa;MAAIC,OAAO,EAAC,EAAE;MAACC,YAAY,EAAC;IAAE,CAAC,EAC7H;MAACN,KAAK,EAAC;MAAGD,QAAQ,EAAE,eAAe;MAAET,IAAI,EAAC,KAAK;MAAEa,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC,oBAAmB;MAAIC,OAAO,EAAC,+BAA+B;MAACC,YAAY,EAAC;IAAE,CAAC,EAChK;MAACN,KAAK,EAAC;MAAGD,QAAQ,EAAE,eAAe;MAAEG,IAAI,EAAC,KAAK;MAAEC,QAAQ,EAAC,eAAe;MAAEC,KAAK,EAAC,cAAa;MAAIC,OA