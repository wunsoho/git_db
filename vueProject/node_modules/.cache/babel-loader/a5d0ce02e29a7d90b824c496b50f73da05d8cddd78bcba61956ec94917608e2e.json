{"ast":null,"code":"import { useStore } from \"vuex\";\nimport { ref, computed } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"power_usage\",\n  setup() {\n    const store = useStore();\n    var unit_s = ref('L');\n    var main_agent = ref('기업');\n    var selected = computed(() => store.state.selected_row);\n    var fule_info_list = [{\n      name: '희발유',\n      unit: 'L'\n    }, {\n      name: '경유',\n      unit: 'L'\n    }, {\n      name: 'LPG',\n      unit: 'L'\n    }, {\n      name: '등유',\n      unit: 'L'\n    }, {\n      name: '윤활유',\n      unit: 'L'\n    }, {\n      name: 'CNG',\n      unit: 'Nm^3'\n    }, {\n      name: 'LNG',\n      unit: 'Nm^3'\n    }];\n    async function click_edit_btn(unit_s) {\n      var info_list = {\n        content: \"\",\n        data: \"\",\n        emissions: \"\",\n        StartDate: \"\",\n        EndDate: \"\",\n        scope: \"1\",\n        category: \"1\"\n      };\n      var usage_input = document.getElementById('amount_fuel').value;\n      info_list.content = document.getElementById('carbon_emissions_content_mobile').value;\n      info_list.data = usage_input + \"/\" + unit_s;\n      info_list.emissions = usage_input + 4;\n      info_list.StartDate = document.getElementById('start_data').value + '-01';\n      info_list.EndDate = document.getElementById('end_data').value + '-01';\n      if (main_agent.value == '기업') {\n        info_list.scope = '1';\n      } else if (main_agent.value == \"민간\") {\n        info_list.scope = '2';\n      }\n      var plz = {\n        \"CarbonData\": {\n          \"StartDate\": document.getElementById('start_data').value + '-01',\n          \"EndDate\": document.getElementById('end_data').value + '-01',\n          \"Location\": \"\",\n          \"Scope\": Number(info_list.scope),\n          \"CarbonActivity\": document.getElementById('carbon_emissions_content_mobile').value,\n          \"CarbonUnit\": unit_s,\n          \"usage\": usage_input + \"/\" + unit_s,\n          \"Chief\": null\n        },\n        \"DetailType\": \"이동연소\"\n        //\"RootCom\":\"samsung\",\n        //\"BelongCom\":\"\",\n      };\n\n      var table = computed(() => store.state.table_kind);\n      console.log(\"테이블 종류\", table.value);\n      if (table.value == 'total_table') {\n        var config = {\n          headers: {\n            \"Authorization\": \"Bearer\" + \" \" + store.state.accessToken\n          }\n        };\n        //console.log(selected.value[0])\n        console.log(plz);\n        await axios.put(\"/CarbonEmission/\" + selected.value[0].id, plz, config).then(res => {\n          console.log(plz);\n        }).catch(error => {\n          alert(\"다시 시도해주세요.\");\n          console.log(error);\n          //router.push('/');\n        }).finally(() => {\n          console.log(\"lender1\");\n        });\n        //수정 API 연결\n      } else if (table.value == 'table') {\n        store.commit(\"SetTableContent\", info_list);\n        store.commit('DelTableContent', selected.value);\n      }\n      store.commit(\"SetEditDelet\");\n      location.reload();\n    }\n    function click_del_editPopup() {\n      console.log('수정창 닫기');\n      store.commit(\"SetEditDelet\");\n    }\n    return {\n      unit_s,\n      main_agent,\n      fule_info_list,\n      click_edit_btn,\n      click_del_editPopup\n    };\n  }\n};","map":{"version":3,"names":["useStore","ref","computed","axios","name","setup","store","unit_s","main_agent","selected","state","selected_row","fule_info_list","unit","click_edit_btn","info_list","content","data","emissions","StartDate","EndDate","scope","category","usage_input","document","getElementById","value","plz","Number","table","table_kind","console","log","config","headers","accessToken","put","id","then","res","catch","error","alert","finally","commit","location","reload","click_del_editPopup"],"sources":["C:\\Users\\ohminjin\\Desktop\\캡스톤\\co2-emission-management\\vueProject\\src\\components\\measure\\EditDivide-carbon\\EditMobile_combustion.vue"],"sourcesContent":["<!--탄소 배출 내용 입력의 이동연소 부분 -->\r\n<template>\r\n    <div>\r\n        <div class=\"edit_top \">\r\n            <span class=\"edit_category\">이동연소 정보 추가하기</span>\r\n            <button class=\"del_edit_btn\" @click=\"click_del_editPopup()\">x</button>\r\n        </div>\r\n        <div style=\"background:#ffffff; padding:3vh; width:25vw; height:70vh; font-size:1.8vh; overflow:auto\">\r\n            <div>\r\n                <div style=\"color:#000000; \">\r\n                탄소 배출 내용<br>   \r\n                <textarea class=\"addInfo_input\" id=\"carbon_emissions_content_mobile\" rows=\"8\" style=\"height:12vh; width:25vw\"></textarea>\r\n                </div> \r\n                <div style=\"margin-top:30px; width:25vw\">기간 설정<br>\r\n                    <input class = \"date_btn\" id = \"start_data\" type=\"month\" data-placeholder=\"시작 날짜\" required aria-required=\"true\" style=\"margin-top:2vh; margin-left:0px; height:3.5vh\">\r\n                    <input class = \"date_btn\" id = \"end_data\" type=\"month\" style=\"margin-left:3vw; height:3.5vh\">\r\n                </div>\r\n                <div style=\"margin-top:30px; font-size:1.8vh\">구분\r\n                    <div class=\"add_info_divide\" id=\"building_name_text\" style=\"margin-top:4vh; width:25vw;\">건물명 / 배출 시설명\r\n                        <input type=\"text\" class=\"addInfo_input\" id =\"building_name_input\" style=\"width:11.5vw; height:3.5vh\" placeholder=\"경상대 본관\">\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"margin-top:2vh\">차량분류\r\n                        <select class=\"addInfo_input\" id=\"operating_entity_input\" style=\"width:12vw\">\r\n                            <option value=\"0\">승용차</option>\r\n                            <option value=\"1\">승합차</option>\r\n                            <option value=\"2\">화물차</option>\r\n                            <option value=\"3\">이륜차</option>\r\n                            <option value=\"4\">비도로 및 기타</option>\r\n                        </select>\r\n                        \r\n                    </div>\r\n                    <div class=\"add_info_divide\">설비명\r\n                        <select v-model=\"main_agent\" class=\"addInfo_input\" id=\"facility_name_input\" style=\"width:12vw\">\r\n                            <option value=\"기업\">기업 소유 및 운영</option>\r\n                            <option value=\"민간\">민간 임차</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\">연료정보\r\n                        <select v-model=\"unit_s\" class=\"addInfo_input\" id=\"fuel_info\" style=\"width:12vw\">\r\n                            <option v-for = \"fule in fule_info_list\" :value=\"fule.unit\">{{fule.name}}</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\" >연료량\r\n                        <input class=\"addInfo_input\" id=\"amount_fuel\" placeholder=\"12,456\" style=\"width:8.8vw\">\r\n                        <select class=\"addInfo_input\" id=\"power_usage_drop\" v-if=\"unit_s=='L'\" style=\"width:2.5vw\">\r\n                            <option value=\"L\">L</option>\r\n                        </select>\r\n                        <select class=\"addInfo_input\" id=\"power_usage_drop\" v-else-if=\"unit_s=='Nm^3'\" style=\"width:2.5vw\">\r\n                            <option value=\"Nm^3\">Nm^3</option>\r\n                        </select>  \r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"edit_bottom\" style=\"font-size:1.5vh\">\r\n            <button class=\"edit_regi_btn\" id=\"edit_regi\" @click=\"click_edit_btn(unit_s)\">수정하기</button>\r\n            <button class=\"edit_regi_btn\" id=\"edit_cancle\" @click=\"click_del_editPopup()\">취소</button>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<style>\r\n   \r\n</style>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\"\r\nimport {ref,computed} from \"vue\"\r\nimport axios from \"axios\";\r\n    export default {\r\n        name :\"power_usage\",\r\n        setup(){\r\n            const store = useStore()\r\n            var unit_s = ref('L')\r\n            var main_agent = ref('기업')\r\n            var selected = computed(()=>store.state.selected_row);\r\n            var fule_info_list = [\r\n                {name: '희발유', unit:'L'},\r\n                {name: '경유', unit:'L'},\r\n                {name: 'LPG', unit:'L'},\r\n                {name: '등유', unit:'L'},\r\n                {name: '윤활유', unit:'L'},\r\n                {name: 'CNG', unit:'Nm^3'},\r\n                {name: 'LNG', unit:'Nm^3'},\r\n            ]\r\n            async function click_edit_btn(unit_s){\r\n                var info_list={content:\"\",data:\"\",emissions:\"\",StartDate:\"\",EndDate:\"\",scope:\"1\",category:\"1\"}\r\n                var usage_input = document.getElementById('amount_fuel').value\r\n                info_list.content = document.getElementById('carbon_emissions_content_mobile').value\r\n                info_list.data =  usage_input+\"/\"+unit_s\r\n                info_list.emissions = usage_input+4\r\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\r\n                info_list.EndDate = document.getElementById('end_data').value+'-01'\r\n                if(main_agent.value == '기업'){\r\n                    info_list.scope = '1'\r\n                }\r\n                else if(main_agent.value == \"민간\"){\r\n                    info_list.scope = '2'\r\n                }\r\n\r\n                var plz = {\r\n                        \"CarbonData\": {\r\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\r\n                            \"EndDate\":document.getElementById('end_data').value+'-01',\r\n                            \"Location\": \"\",\r\n                            \"Scope\":  Number(info_list.scope),\r\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_content_mobile').value,\r\n                            \"CarbonUnit\": unit_s,\r\n                            \"usage\": usage_input+\"/\"+unit_s,\r\n                            \"Chief\": null\r\n                        },\r\n                        \"DetailType\":\"이동연소\",\r\n                        //\"RootCom\":\"samsung\",\r\n                        //\"BelongCom\":\"\",\r\n                }\r\n                var table = computed(() => store.state.table_kind)\r\n                console.log(\"테이블 종류\",table.value)\r\n                if(table.value == 'total_table'){\r\n                    var config = {\r\n                        headers:{\r\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\r\n                        }\r\n                    }\r\n                   //console.log(selected.value[0])\r\n                   console.log(plz)\r\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\r\n                        console.log(plz)\r\n                        \r\n                    })\r\n                    .catch(error => {\r\n                        alert(\"다시 시도해주세요.\")\r\n                        console.log(error)\r\n                        //router.push('/');\r\n                    })\r\n                    .finally(() => {\r\n                        console.log(\"lender1\")\r\n                    })\r\n                    //수정 API 연결\r\n                }\r\n                else if(table.value == 'table'){\r\n                    store.commit(\"SetTableContent\",info_list);\r\n                    store.commit('DelTableContent',selected.value);\r\n                }\r\n                store.commit(\"SetEditDelet\");\r\n                location.reload();\r\n            }\r\n            \r\n            function click_del_editPopup(){\r\n                console.log('수정창 닫기')\r\n                store.commit(\"SetEditDelet\");\r\n            }\r\n            return{\r\n                unit_s,\r\n                main_agent,\r\n                fule_info_list,\r\n                click_edit_btn,\r\n                click_del_editPopup\r\n            }\r\n        },\r\n    }\r\n</script>"],"mappings":"AAmEA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,GAAG,EAACC,QAAQ,QAAO,KAAI;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,aAAa;EACnBC,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,MAAK,GAAIN,GAAG,CAAC,GAAG;IACpB,IAAIO,UAAS,GAAIP,GAAG,CAAC,IAAI;IACzB,IAAIQ,QAAO,GAAIP,QAAQ,CAAC,MAAII,KAAK,CAACI,KAAK,CAACC,YAAY,CAAC;IACrD,IAAIC,cAAa,GAAI,CACjB;MAACR,IAAI,EAAE,KAAK;MAAES,IAAI,EAAC;IAAG,CAAC,EACvB;MAACT,IAAI,EAAE,IAAI;MAAES,IAAI,EAAC;IAAG,CAAC,EACtB;MAACT,IAAI,EAAE,KAAK;MAAES,IAAI,EAAC;IAAG,CAAC,EACvB;MAACT,IAAI,EAAE,IAAI;MAAES,IAAI,EAAC;IAAG,CAAC,EACtB;MAACT,IAAI,EAAE,KAAK;MAAES,IAAI,EAAC;IAAG,CAAC,EACvB;MAACT,IAAI,EAAE,KAAK;MAAES,IAAI,EAAC;IAAM,CAAC,EAC1B;MAACT,IAAI,EAAE,KAAK;MAAES,IAAI,EAAC;IAAM,CAAC,CAC9B;IACA,eAAeC,cAAcA,CAACP,MAAM,EAAC;MACjC,IAAIQ,SAAS,GAAC;QAACC,OAAO,EAAC,EAAE;QAACC,IAAI,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,KAAK,EAAC,GAAG;QAACC,QAAQ,EAAC;MAAG;MAC7F,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC,CAACC,KAAI;MAC7DX,SAAS,CAACC,OAAM,GAAIQ,QAAQ,CAACC,cAAc,CAAC,iCAAiC,CAAC,CAACC,KAAI;MACnFX,SAAS,CAACE,IAAG,GAAKM,WAAW,GAAC,GAAG,GAAChB,MAAK;MACvCQ,SAAS,CAACG,SAAQ,GAAIK,WAAW,GAAC;MAClCR,SAAS,CAACI,SAAQ,GAAIK,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtEX,SAAS,CAACK,OAAM,GAAII,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAI;MAClE,IAAGlB,UAAU,CAACkB,KAAI,IAAK,IAAI,EAAC;QACxBX,SAAS,CAACM,KAAI,GAAI,GAAE;MACxB,OACK,IAAGb,UAAU,CAACkB,KAAI,IAAK,IAAI,EAAC;QAC7BX,SAAS,CAACM,KAAI,GAAI,GAAE;MACxB;MAEA,IAAIM,GAAE,GAAI;QACF,YAAY,EAAE;UACV,WAAW,EAACH,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC7D,SAAS,EAACF,QAAQ,CAACC,cAAc,CAAC,U