og(contentList ,'저장')\r\n                closeDetailAddPopup()\r\n            }\r\n\r\n            //내용 추가\r\n\r\n            //내용 추가하기 버튼 클릭시\r\n            var addInput = ref(false)\r\n            console.log(addInput.value)\r\n            function addCategory(){\r\n                addInput.value=true\r\n            }\r\n\r\n            //내용 입력후 엔터키 누를시 \r\n            var incomeAddContent = ref('')\r\n            function completeAddCategory(){\r\n                if(incomeAddContent.value == ''){\r\n\r\n                }\r\n                else{\r\n                    contentList.push({kind:incomeAddContent.value, check:false})\r\n                    incomeAddContent.value=''\r\n                }\r\n                \r\n                addInput.value=false\r\n            }\r\n            return{\r\n                clickOk,\r\n                closeDetailAddPopup,\r\n                contentList,\r\n                addCategory,\r\n                completeAddCategory,\r\n                addInput,\r\n                incomeAddContent\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n.popup-kind-block{\r\n    border:1px solid #949494;\r\n    height:3vh;\r\n    width:inherit;\r\n    margin-top:1vh;\r\n    padding:0.2vh 2vw;\r\n    padding-left:0;\r\n}\r\n.kind-content{\r\n    font-size:1.5vh;\r\n    width:21.9vw;\r\n    height:3vh;\r\n    line-height:3vh;\r\n    color:#1f1f1f;\r\n    font-weight: 600;\r\n}\r\n.edit-category-add{\r\n    margin-top:2vh;\r\n    font-weight: 900;\r\n    color:#5F5F5F\r\n}\r\n.edit-category-add:hover{\r\n    cursor:pointer;\r\n}\r\n</style>"],"mappings":";AA6BA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAASC,QAAO,QAAS,MAAM;AAE3B,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,WAAU,GAAI,CACd;MAACC,IAAI,EAAC,MAAM;MAAEC,KAAK,EAAC;IAAK,CAAC,EAC1B;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAC;IAAK,CAAC,EACxB;MAACD,IAAI,EAAC,KAAK;MAAEC,KAAK,EAAC;IAAK,CAAC,CAC7B;IACA,IAAIC,KAAI,GAAIP,QAAQ,EAAE;;IAEtB;IA