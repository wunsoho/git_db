          scope12CarbonEmission.value = res.data.Scopes[0]+res.data.Scopes[1]\r\n                scope3CarbonEmission.value = res.data.Scopes[2]\r\n            })\r\n            .catch(error => {\r\n                \r\n                console.log(error)\r\n                \r\n            })\r\n            .finally(() => {\r\n            })\r\n        }\r\n        \r\n\r\n        var TotalEmission = ref(scope3CarbonEmission.value + scope12CarbonEmission.value)\r\n\r\n        return{Offpreview,GroupName,TotalEmission,scope12CarbonEmission,scope3CarbonEmission,\r\n            OnTotalPreview,OnDetailPreview,OnInfoPreview,get_total_emission}\r\n    },\r\n    created(){\r\n        this.get_total_emission()\r\n    },\r\n    mounted(){},\r\n    unmounted(){},\r\n    methods:{}\r\n}\r\n</script>"],"mappings":"AA8IA,SAASA,QAAQ,EAACC,GAAE,QAAS,KAAK;AAClC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAc;EACVC,IAAI,EAAC,EAAE;EACPC,UAAU,EAAC,CAAC,CAAC;EACbC,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAE,EAAE;IAC1B,MAAMM,UAAS,GAAIA,CAAA,KAAMD,KAAK,CAACE,MAAM,CAAC,iBAAiB,CAAG;IAC1D,MAAMC,cAAa,GAAIA,CAAA,KAAMH,KAAK,CAACE,MAAM,CAAC,gBAAgB,EAAE,OAAO,CAAC;IACpE,MAAME,eAAc,GAAIA,CAAA,KAAMJ,KAAK,CAACE,MAAM,CAAC,gBAAgB,EAAE,QAAQ,CAAC;IACtE,MAAMG,aAAY,GAAIA,CAAA,KAAML,KAAK,CAACE,MAAM,CAAC,gBAAgB,EAAE,MAAM,CAAC;IAClE,IAAII,KAAI,GAAI,IAAIC,IAAI,EAAE;IACtB,IAAIC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAC;IAE7B,IAAIC,SAAQ,GAAGhB,GAAG,CAAC,EAAE;IACrBgB,SAAS,GAAEjB,QAAQ,CAAC,MAAMO,KAAK,CAACW,KAAK,CAACC,2BAA2B,CAAC;IAElE,IAAIC,qBAAoB,GAAInB,GAAG,CAAC,CAAC,CAAC;IAClC,IAAIoB,oBAAmB,GAAIpB,GAAG,CAAC,CAAC,CAAC;IACjC,IAAIqB,KAAI,GAAItB,QAAQ,CAAC,MAAMO,KAAK,CAACW,KAAK,CAACK,aAAa,GAAC,CAAC,CAAC;IACvD,IAAIC,MAAK,GAAI;MACTC,OAAO,EAAC;QACR,eAAe,EAAC,QAAQ,GAAC,GAAG,GAAClB,KAAK,CAACW,KAAK,CAACQ;MAEzC;IACJ;IACA,eAAeC,kBAAkBA,CAAA,EAAE;MAC/B,MAAMxB,KAAK,CAACyB,GAAG,CAAC,kBAAkB,GAACrB,KAAK,CAACW,KAAK,CAACC,2BAA2B,GAAC,GAAG,GAACJ,IAAI,GAAC,SAAS,GAACA,IAAI,GAAC,QAAQ,EAACS,MAAM,CAAC,CAACK,IAAI,CAACC,GAAE,IAAK;QAC5