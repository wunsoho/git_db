e_input+4\r\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\r\n                info_list.EndDate = document.getElementById('end_data').value+'-01'\r\n\r\n                var plz = {\r\n                        \"CarbonData\": {\r\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\r\n                            \"EndDate\":document.getElementById('end_data').value+'-01',\r\n                            \"Location\": \"\",\r\n                            \"Scope\":  Number(info_list.scope),\r\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_conten_wasteWater').value,\r\n                            \"CarbonUnit\": unit_s,\r\n                            \"usage\": usage_input+\"/\"+unit_s,\r\n                            \"Chief\": null\r\n                        },\r\n                        \"DetailType\":\"폐기물 처리시설\",\r\n                        //\"RootCom\":\"samsung\",\r\n                        //\"BelongCom\":\"\",\r\n                }\r\n                var table = computed(() => store.state.table_kind)\r\n                console.log(\"테이블 종류\",table)\r\n                if(table.value == 'total_table'){\r\n                    //수정 API 연결\r\n                    var config = {\r\n                        headers:{\r\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\r\n                        }\r\n                    }\r\n                   //console.log(selected.value[0])\r\n                   console.log(plz)\r\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\r\n                        console.log(plz)\r\n                        \r\n                    })\r\n                    .catch(error => {\r\n                        alert(\"다시 시도해주세요.\")\r\n                        console.log(error)\r\n                        //router.push('/');\r\n                    })\r\n                    .finally(() => {\r\n                        console.log(\"lender1\")\r\n                    })\r\n                }\r\n                else if(table.value == 'table'){\r\n                    store.commit(\"SetTableContent\",info_list);\r\n                    store.commit('DelTableContent',selected.value);\r\n                }\r\n                store.commit(\"SetEditDelet\");\r\n                location.reload();\r\n            }\r\n            \r\n            function click_del_editPopup(){\r\n                console.log('수정창 닫기')\r\n                store.commit(\"SetEditDelet\");\r\n            }\r\n            return{\r\n                click_edit_btn,\r\n                click_del_editPopup\r\n            }\r\n        }\r\n    }\r\n</script>"],"mappings":"AA6EA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,GAAG,EAACC,QAAQ,QAAO,KAAI;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,2BAA2B;EACjCC,KAAKA,CAAA,EAAE;IACH,IAAIC,QAAO,GAAIJ,QAAQ,CAAC,MAAIK,KAAK,CAACC,KAAK,CAACC,YAAY,CAAC;IACrD,MAAMF,KAAI,GAAIP,QAAQ,EAAC;IACvB,eAAeU,cAAcA,CAAA,EAAE;MAC3B,IAAIC,SAAQ,GAAI;QAACC,OAAO,EAAC,EAAE;QAACC,IAAI,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,KAAK,EAAC,GAAG;QAACC,QAAQ,EAAC;MAAI;MAChG,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,mBAAmB,CAAC,CAACC,KAAI;MACnEX,SAAS,CAACC,OAAM,GAAIQ,QAAQ,CAACC,cAAc,CAAC,oCAAoC,CAAC,CAACC,KAAI;MACtFX,SAAS,CAACE,IAAG,GAAKM,WAAW,GAAC,GAAG,GAAC,KAAI;MACtCR,SAAS,CAACG,SAAQ,GAAIK,WAAW,GAAC;MAClCR,SAAS,CAACI,SAAQ,GAAIK,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtEX,SAAS,CAACK,OAAM,GAAII,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAI;MAElE,IAAIC,GAAE,GAAI;QACF,YAAY,EAAE;UACV,WAAW,EAACH,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC7D,SAAS,EAACF,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAK;UACzD,UAAU,EAAE,EAAE;UACd,OAAO,EAAGE,MAAM,CAACb,SAAS,CAACM,KAAK,CAAC;UACjC,gBAAgB,EAAEG,QAAQ,CAACC,cAAc,CAAC,oCAAoC,CAAC,CAACC,KAAK;UACrF,YAAY,EAAEG,MAAM;UACpB,OAAO,EAAEN,WAAW,GAAC,