{"ast":null,"code":"import { useStore } from 'vuex';\nimport { ref, computed } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: \"steam_usage\",\n  setup() {\n    const store = useStore();\n    var unit_s = ref(\"GJ\");\n    var main_agent = ref('기업');\n    var selected = computed(() => store.state.selected_row);\n    async function click_edit_btn(unit_s) {\n      console.log(\"수도 변경\");\n      var info_list = {\n        id: \"\",\n        content: \"\",\n        data: \"\",\n        emissions: \"\",\n        StartDate: \"\",\n        EndDate: \"\",\n        scope: \"\",\n        category: \"8\"\n      };\n      var usage_input = document.getElementById('steam_usage_input').value;\n      info_list.content = document.getElementById('carbon_emissions_content_steam2').value;\n      info_list.data = usage_input + \"/\" + unit_s;\n      info_list.emissions = usage_input + 4 + \"kg\"; //계산식\n      info_list.StartDate = document.getElementById('start_data').value + '-01';\n      info_list.EndDate = document.getElementById('end_data').value + '-01';\n      info_list.id = selected.value[0].id;\n      if (main_agent.value == '기업') {\n        info_list.scope = '1';\n      } else if (main_agent.value == \"민간\") {\n        info_list.scope = '2';\n      }\n      var plz = {\n        \"CarbonData\": {\n          \"StartDate\": document.getElementById('start_data').value + '-01',\n          \"EndDate\": document.getElementById('end_data').value + '-01',\n          \"Location\": \"\",\n          \"Scope\": Number(info_list.scope),\n          \"CarbonActivity\": document.getElementById('carbon_emissions_content_steam2').value,\n          \"CarbonUnit\": unit_s,\n          \"usage\": usage_input + \"/\" + unit_s,\n          \"Chief\": null\n        },\n        \"DetailType\": \"열\"\n        //\"RootCom\":\"samsung\",\n        //\"BelongCom\":\"\",\n      };\n\n      var table = computed(() => store.state.table_kind);\n      console.log(\"테이블 종류\", table.value);\n      if (table.value == 'total_table') {\n        //수정 API 연결\n        var config = {\n          headers: {\n            \"Authorization\": \"Bearer\" + \" \" + store.state.accessToken\n          }\n        };\n        //console.log(selected.value[0])\n        console.log(plz);\n        await axios.put(\"/CarbonEmission/\" + selected.value[0].id, plz, config).then(res => {\n          console.log(plz);\n        }).catch(error => {\n          alert(\"다시 시도해주세요.\");\n          console.log(error);\n          //router.push('/');\n        }).finally(() => {\n          console.log(\"lender1\");\n        });\n      } else if (table.value == 'table') {\n        store.commit(\"SetTableContent\", info_list);\n        store.commit('DelTableContent', selected.value);\n      }\n      store.commit(\"SetEditDelet\");\n      location.reload();\n    }\n    function click_del_editPopup() {\n      console.log('수정창 닫기');\n      store.commit(\"SetEditDelet\");\n    }\n    return {\n      unit_s,\n      main_agent,\n      click_edit_btn,\n      click_del_editPopup\n    };\n  }\n};","map":{"version":3,"names":["useStore","ref","computed","axios","name","setup","store","unit_s","main_agent","selected","state","selected_row","click_edit_btn","console","log","info_list","id","content","data","emissions","StartDate","EndDate","scope","category","usage_input","document","getElementById","value","plz","Number","table","table_kind","config","headers","accessToken","put","then","res","catch","error","alert","finally","commit","location","reload","click_del_editPopup"],"sources":["C:\\Users\\User\\Desktop\\대학\\4학년 1학기\\탄소\\co2-emission-management\\vueProject\\src\\components\\measure\\EditDivide-carbon\\EditSteam_usage.vue"],"sourcesContent":["<!-- 열 사용-->\r\n<template>\r\n    <div>\r\n        <div class=\"edit_top \">\r\n            <span class=\"edit_category\">열 사용 정보 추가하기</span>\r\n            <button class=\"del_edit_btn\" @click=\"click_del_editPopup()\">x</button>\r\n        </div>\r\n        <div style=\"background:#ffffff; padding:3vh; width:25vw; height:70vh; font-size:1.8vh; overflow:auto\">\r\n            <div>\r\n                <div style=\"color:#000000; \">\r\n                탄소 배출 내용<br>   \r\n                <textarea class=\"addInfo_input\" id=\"carbon_emissions_content_steam2\" rows=\"8\" style=\"height:12vh; width:25vw\"></textarea>\r\n                </div> \r\n                <div style=\"margin-top:30px; width:25vw\">기간 설정<br>\r\n                    <input class = \"date_btn\" id = \"start_data\" type=\"month\" data-placeholder=\"시작 날짜\" required aria-required=\"true\" style=\"margin-top:2vh; margin-left:0px; height:3.5vh\">\r\n                    <input class = \"date_btn\" id = \"end_data\" type=\"month\" style=\"margin-left:3vw; height:3.5vh\">\r\n                </div>\r\n                <div style=\"margin-top:30px; font-size:1.8vh\">구분\r\n                    <div class=\"add_info_divide\" id=\"building_name_text\" style=\"margin-top:4vh; width:25vw;\">건물명 / 배출 시설명\r\n                        <input type=\"text\" class=\"addInfo_input\" id =\"building_name_input\" style=\"width:11.5vw; height:3.5vh\" placeholder=\"경상대 본관\">\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">운영주체\r\n                        <select v-model=\"main_agent\" class=\"addInfo_input\" id=\"operating_entity_input\" style=\"width:12vw; height:4.2vh; margin-left:4.3vw\">\r\n                            <option value=\"기업\">기업 소유 및 운영</option>\r\n                            <option value=\"민간\">민간 임차</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">공급처\r\n                        <select class=\"addInfo_input\" id=\"supplier_drop\" style=\"width:12vw; height:4.2vh; margin-left:5.2vw\">\r\n                            <option value=\"0\">한국지역난방공사</option>\r\n                            <option value=\"1\">직접 입력</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\">연료 종류\r\n                        <select class=\"addInfo_input\" id=\"supplier_drop_steam\" style=\"width:12vw\">\r\n                            <option value=\"0\">스팀(열전용)</option>\r\n                            <option value=\"1\">스팀(열병합)</option>\r\n                            <option value=\"2\">스팀(일반)</option>\r\n                        </select>\r\n                    </div>\r\n                    <div class=\"add_info_divide\" style=\"font-size:1.8vh\">열 사용량\r\n                        <input class=\"addInfo_input\" id=\"steam_usage_input\" placeholder=\"12,456\" style=\"margin-left:4vw; width:9vw; height:4vh\">\r\n                        <select v-model=\"unit_s\" class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"width:2.6vw\">\r\n                            <option value=\"GJ\">GJ</option>\r\n                            <option value=\"MJ\">MJ</option>\r\n                        </select>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"edit_bottom\" style=\"font-size:1.5vh\">\r\n            <button class=\"edit_regi_btn\" id=\"edit_regi\" @click=\"click_edit_btn(unit_s)\">수정하기</button>\r\n            <button class=\"edit_regi_btn\" id=\"edit_cancle\" @click=\"click_del_editPopup()\">취소</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<style>\r\n\r\n    #supplier_drop_steam{\r\n        width: 20%;\r\n        margin-left: 90px;\r\n        color: #727374\r\n    }\r\n\r\n    #steam_usage_input{\r\n        margin-left:90px;\r\n        width:14%;\r\n        background: #ffffff;\r\n        border: 1px solid #DDE2E5;\r\n    }\r\n\r\n    #steam_usage_drop{\r\n        width: 4.6%;\r\n        margin-left: 1%;\r\n        color: #727374;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n   \r\n</style>\r\n\r\n<script>\r\nimport { useStore } from 'vuex'\r\nimport { ref,computed } from 'vue'\r\nimport axios from \"axios\";\r\n    export default {\r\n        name :\"steam_usage\",\r\n        setup(){\r\n            const store = useStore()\r\n            var unit_s = ref(\"GJ\")\r\n            var main_agent = ref('기업')\r\n            var selected = computed(()=>store.state.selected_row);\r\n            async function click_edit_btn(unit_s){\r\n                console.log(\"수도 변경\")\r\n                var info_list={id:\"\",content:\"\",data:\"\",emissions:\"\",StartDate:\"\",EndDate:\"\",scope:\"\",category:\"8\"}\r\n                var usage_input = document.getElementById('steam_usage_input').value\r\n                info_list.content = document.getElementById('carbon_emissions_content_steam2').value\r\n                info_list.data =  usage_input+\"/\"+unit_s\r\n                info_list.emissions = usage_input + 4 + \"kg\" //계산식\r\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\r\n                info_list.EndDate = document.getElementById('end_data').value+'-01'\r\n                info_list.id = selected.value[0].id\r\n                if(main_agent.value == '기업'){\r\n                    info_list.scope = '1'\r\n                }\r\n                else if(main_agent.value == \"민간\"){\r\n                    info_list.scope = '2'\r\n                }\r\n\r\n                var plz = {\r\n                        \"CarbonData\": {\r\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\r\n                            \"EndDate\":document.getElementById('end_data').value+'-01',\r\n                            \"Location\": \"\",\r\n                            \"Scope\":  Number(info_list.scope),\r\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_content_steam2').value,\r\n                            \"CarbonUnit\": unit_s,\r\n                            \"usage\": usage_input+\"/\"+unit_s,\r\n                            \"Chief\": null\r\n                        },\r\n                        \"DetailType\":\"열\",\r\n                        //\"RootCom\":\"samsung\",\r\n                        //\"BelongCom\":\"\",\r\n                }\r\n                var table = computed(() => store.state.table_kind)\r\n                console.log(\"테이블 종류\",table.value)\r\n                if(table.value == 'total_table'){\r\n                   //수정 API 연결\r\n                   var config = {\r\n                        headers:{\r\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\r\n                        }\r\n                    }\r\n                   //console.log(selected.value[0])\r\n                   console.log(plz)\r\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\r\n                        console.log(plz)\r\n                        \r\n                    })\r\n                    .catch(error => {\r\n                        alert(\"다시 시도해주세요.\")\r\n                        console.log(error)\r\n                        //router.push('/');\r\n                    })\r\n                    .finally(() => {\r\n                        console.log(\"lender1\")\r\n                    })\r\n                    \r\n                }\r\n                else if(table.value == 'table'){\r\n                    store.commit(\"SetTableContent\",info_list);\r\n                    store.commit('DelTableContent',selected.value);\r\n                }\r\n                store.commit(\"SetEditDelet\");\r\n                location.reload();\r\n            }\r\n\r\n            function click_del_editPopup(){\r\n                console.log('수정창 닫기')\r\n                store.commit(\"SetEditDelet\");\r\n            }\r\n\r\n            return{\r\n                unit_s,\r\n                main_agent,\r\n                click_edit_btn,\r\n                click_del_editPopup\r\n            }\r\n        }\r\n    }\r\n</script>"],"mappings":"AAmFA,SAASA,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAG,EAACC,QAAO,QAAS,KAAI;AACjC,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,aAAa;EACnBC,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,MAAK,GAAIN,GAAG,CAAC,IAAI;IACrB,IAAIO,UAAS,GAAIP,GAAG,CAAC,IAAI;IACzB,IAAIQ,QAAO,GAAIP,QAAQ,CAAC,MAAII,KAAK,CAACI,KAAK,CAACC,YAAY,CAAC;IACrD,eAAeC,cAAcA,CAACL,MAAM,EAAC;MACjCM,OAAO,CAACC,GAAG,CAAC,OAAO;MACnB,IAAIC,SAAS,GAAC;QAACC,EAAE,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,IAAI,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,KAAK,EAAC,EAAE;QAACC,QAAQ,EAAC;MAAG;MAClG,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,mBAAmB,CAAC,CAACC,KAAI;MACnEZ,SAAS,CAACE,OAAM,GAAIQ,QAAQ,CAACC,cAAc,CAAC,iCAAiC,CAAC,CAACC,KAAI;MACnFZ,SAAS,CAACG,IAAG,GAAKM,WAAW,GAAC,GAAG,GAACjB,MAAK;MACvCQ,SAAS,CAACI,SAAQ,GAAIK,WAAU,GAAI,IAAI,IAAG,EAAE;MAC7CT,SAAS,CAACK,SAAQ,GAAIK,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtEZ,SAAS,CAACM,OAAM,GAAII,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAI;MAClEZ,SAAS,CAACC,EAAC,GAAIP,QAAQ,CAACkB,KAAK,CAAC,CAAC,CAAC,CAACX,EAAC;MAClC,IAAGR,UAAU,CAACmB,KAAI,IAAK,IAAI,EAAC;QACxBZ,SAAS,CAACO,KAAI,GAAI,GAAE;MACxB,OACK,IAAGd,UAAU,CAACmB,KAAI,IAAK,IAAI,EAAC;QAC7BZ,SAAS,CAACO,KAAI,GAAI,GAAE;MACxB;MAEA,IAAIM,GAAE,GAAI;QACF,YAAY,EAAE;UACV,WAAW,EAACH,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC7D,SAAS,EAACF,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAK;UACzD,UAAU,EAAE,EAAE;UACd,OAAO,EAAGE,MAAM,CAACd,SAAS,CAACO,KAAK,CAAC;UACjC,gBAAgB,EAAEG,QAAQ,CAACC,cAAc,CAAC,iCAAiC,CAAC,CAACC,KAAK;UAClF,YAAY,EAAEpB,MAAM;UACpB,OAAO,EAAEiB,WAAW,GAAC,GAAG,GAACjB,MAAM;UAC/B,OAAO,EAAE;QACb,CAAC;QACD,YAAY,EAAC;QACb;QACA;MACR;;MACA,IAAIuB,KAAI,GAAI5B,QAAQ,CAAC,MAAMI,KAAK,CAACI,KAAK,CAACqB,UAAU;MACjDlB,OAAO,CAACC,GAAG,CAAC,QAAQ,EAACgB,KAAK,CAACH,KAAK;MAChC,IAAGG,KAAK,CAACH,KAAI,IAAK,aAAa,EAAC;QAC7B;QACA,IAAIK,MAAK,GAAI;UACRC,OAAO,EAAC;YACJ,eAAe,EAAC,QAAQ,GAAC,GAAG,GAAC3B,KAAK,CAACI,KAAK,CAACwB;UAC7C;QACJ;Q