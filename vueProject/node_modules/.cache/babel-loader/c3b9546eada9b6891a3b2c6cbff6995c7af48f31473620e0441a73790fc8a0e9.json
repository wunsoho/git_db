lass=\"{'block popup-kind-block': content.check==false,'click-block':content.check==true}\" @click=\"clickContent(content,i)\">\n                        <div class=\"detail-info kind-content\">{{content.kind}}</div>\n                    </div>\n                </div>\n                {{ addInput }}\n                <div v-if=\"addInput==false\" class=\"edit-category-add\" @click=\"addCategory()\">+ 내용 추가하기</div>\n                <input v-else-if=\"addInput==true\" @keypress=\"completeAddCategory()\">\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {ref} from 'vue'\nimport { useStore } from 'vuex';\n\n    export default{ \n        name:'accountDetailCom',\n        components:{\n        },\n        setup(){\n            var contentList = [\n                {kind:'여가활동', check:false},\n                {kind:'간식', check:false},\n                {kind:'음식점', check:false},\n                {kind:'약', check:false},\n            ]\n            var store = useStore();\n            function closeDetailAddPopup(){\n                console.log('삭제')\n                store.commit('closeEditCategory')\n            }\n            function clickOk(){\n                \n                closeDetailAddPopup()\n            }\n            var addInput = ref(false)\n            console.log(addInput.value)\n            function addCategory(){\n                addInput.value=true\n            }\n            function completeAddCategory(){\n                addInput.value=false\n            }\n            return{\n                clickOk,\n                closeDetailAddPopup,\n                contentList,\n                addCategory,\n                completeAddCategory\n            }\n        }\n    }\n\n\n</script>\n\n<style>\n\n.popup-kind-block{\n    border:1px solid #949494;\n    height:3vh;\n    width:inherit;\n    margin-top:1vh;\n    padding:0.2vh 2vw\n}\n.kind-content{\n    font-size:1.5vh;\n    width:21.9vw;\n    height:3vh;\n    line-height:3vh;\n