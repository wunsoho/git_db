ue,\n                }\n                console.log(\"수정하기\",group_list)\n                edit_group_server(group_list)\n                // store.commit(\"InsightAddY\",-1);\n                store.commit(\"OffGroupEditPopup\");\n            }\n            async function edit_group_server(group_list){\n                var config = {\n                    headers:{\n                    \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\n                    }\n                }\n                console.log(\"수정하기\",group_list)\n                await axios.put(\"Company/PreviewInfo/\"+group_name.value, group_list, config).then(res => {\n                    console.log(res.data)\n                })\n                .catch(error => {\n                    alert(\"로그인 시간이 만료되었습니다.\")\n                    console.log(error)\n                    router.push('/');\n                })\n                .finally(() => {})\n                location.reload();\n            }\n            return {GroupEditBtn,offpopup,group_name,editGroup,depth,edit_group_server}\n        },\n        mounted(){\n        },\n        destroyed(){\n        }\n    };\n</script>"],"mappings":";;;EACSA,KAAK,EAAE,QAAQ;EAACC,EAAE,EAAC;;;EACfD,KAAK,EAAC;AAAU;gCAC8BE,mBAAA,CAAI;;EAC9CC,KAAwE,EAAxE;IAAA;IAAA;IAAA;IAAA;EAAA;AAAwE;;EACpEA,KAAmC,EAAnC;IAAA;IAAA;EAAA;AAAmC;gCAC7BD,mBAAA,CAAI;;EACRC,KAAuE,EAAvE;IAAA;IAAA;IAAA;IAAA;EAAA;AAAuE;;;SANjDC,MAAA,CAAAC,YAAY,Y,cAArDC,mBAAA,CAqBM,OArBNC,UAqBM,GApBFL,mBAAA,CAmBM,OAnBNM,UAmBM,G,iBAnBgB,UAClB,GAAAN,mBAAA,CAA+C;IAAzCD,EAAE,EAAC,SAAS;IAAEQ,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEP,MAAA,CAAAQ,QAAQ;KAAI,GAAC,GAAOC,UAAI,EACnDX,mBAAA,CAeM,OAfNY,UAeM,GAdFZ,mBAAA,CAGM,OAHNa,UAGM,G,iBAHmC,UAC9B,GAAAC,UAAI,EACXd,mBAAA,CAA6F,KAA7Fe,UAA6F,EAAAC,gBAAA,CAAhBd,MAAA,CAAAe,UAAU,iB,GAE3FC,UAA4C,C,GAWhDlB,mBAAA,CAAiE;IAAzDF,KAAK,EAAC,gBAAgB;IAAES,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEP,MAAA,CAAAiB,SAAS;KAAI,MAAI,E"},"metadata":{},"sourceType":"module","externalDepende