ry'; \nimport axios from 'axios';\n    export default{ \n        name:'MemberAddPopup',\n        components:{\n        },\n        setup(){\n            var position = ref(0)\n            const store = useStore(); //vuex 사용\n            var memberAddPopup = computed(()=> store.state.addMember);\n            console.log(memberAddPopup.value);\n            const closeAddMemberPopup = () => store.commit(\"closeAddMember\");\n            var imgFile = ref('dd')\n\n            function saveAddMember(){\n                const formData = new FormData();\n                \n                var input_data = {\n                    Position : position.value,\n                    Name : document.getElementById('member-name').value,\n                    PhoneNum : document.getElementById('member-tel').value,\n                    Img : null,\n                    Email :document.getElementById('member-email').value,\n                    HomePage : document.getElementById('member-hompage').value,\n                    Git: document.getElementById('member-github').value,\n                }\n                \n                formData.append(\"Position\" ,input_data.Position)\n                formData.append(\"Position\" ,input_data.Name)\n                formData.append(\"Position\" ,input_data.PhoneNum)\n                formData.append(\"Position\" ,input_data.Img)\n                formData.append(\"Position\" ,input_data.Email)\n                formData.append(\"Position\" ,input_data.HomePage)\n                formData.append(\"Position\" ,input_data.Git)\n                console.log(formData)\n\n                store.commit(\"closeAddMember\");\n                member_post_list(input_data)\n            }\n\n            async function member_post_list(data){\n                console.log(\"dawdaw\")\n                await axios.post('/member/',data).then(res => {\n                    console.log(this.memberInfo)\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n                .finally(() => {})\n            }\n\n            const readInputFile = (e) => { //미리보기 기능\n                $('#popup_img_preview').empty();\n                var files = e.target.files;\n                imgFile.value=e.target.files[0]\n                console.log(imgFile.value)\n                var fileArr = Array.prototype.slice.call(files);\n                console.log(fileArr)\n                fileArr.forEach(function(f){\n                    if(!f.type.match(\"image/.*\")){\n                        alert(\"이미지 파일만 업로드 가능합니다\")\n                        return;\n                    };\n                    var reader = new FileReader();\n                    reader.onload = function(e){\n                        console.log(e.target.result)\n                        var html = `<img src=${e.target.result} style=max-width:10vw; max-height:20vh;/>`;\n                        $('#popup_img_preview').append(html);\n                    };\n                    reader.readAsDataURL(f);\n                })\n            }\n\n            return{\n                memberAddPopup,\n                saveAddMember,\n                closeAddMemberPopup,\n                readInputFile,\n            }\n            \n        }\n    }\n</script>\n\n<style>\n.middle-text{\n    font-size:1vh;\n}\n.popup{\n    background: rgba(84, 84, 84, 0.7);\n    width:100vw;\n    height:100vh;\n    position:fixed;\n}\n\n.popup > div{\n    width:45vw;\n    height:60vh;\n    background-color:white;\n    margin:auto;\n    margin-top:5vh;\n    border-radius: 1vh;\n    vertical-align: middle;\n    padding:4vh 5vw\n}\n#popup-img{\n    float: left;\n}\n#popup_img_preview{\n    width:10vw;\n    height:25vh;\n    background-color:rgb(187, 187, 187);\n}\n\n#customFile{\n    clear:both;\n    margin-top:2vh;\n}\n.popup-input{\n    display:inline-block;\n    width:7vw;\n    text-align: right;\n    margin:1vh\n}\n</style>"],"mappings":";;;EACqCA,KAAK,EAAG;;;EAE