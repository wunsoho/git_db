= [\n\n                    Position = \n                    Name = document.getElementById('member-name').value,\n                    PhoneNum = document.getElementById('member-tel').value,\n                    Email =document.getElementById('member-tel').value,\n                    HomePage = document.getElementById('member-tel').value,\n                    Git= document.getElementById('member-tel').value,\n\n                ]\n                store.commit(\"closeAddMember\");\n                member_post_list(input_data)\n            }\n            async function member_post_list(data){\n                console.log(\"dawdaw\")\n                await axios.post('/member',data).then(res => {\n                    console.log(this.memberInfo)\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n                .finally(() => {})\n            }\n\n            return{\n                memberAddPopup,\n                closeAddMemberPopup\n            }\n            \n        }\n    }\n</script>\n\n<style>\n.middle-text{\n    font-size:1vh;\n}\n.popup{\n    background: rgba(84, 84, 84, 0.7);\n    width:100vw;\n    height:100vh;\n    position:fixed;\n}\n\n.popup > div{\n    width:45vw;\n    height:60vh;\n    background-color:white;\n    margin:auto;\n    margin-top:5vh;\n    border-radius: 1vh;\n    vertical-align: middle;\n    padding:4vh 5vw\n}\n#popup_img_preview{\n    width:10vw;\n    height:25vh;\n    background-color:rgb(187, 187, 187);\n    float: left;\n}\n.popup-input{\n    display:inline-block;\n    width:7vw;\n    text-align: right;\n    margin:1vh\n}\n</style>"],"mappings":"AAgCA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,GAAG,EAACC,QAAO,QAAS,KAAI;AAC7B,eAAc;EACVC,IAAI,EAAC,gBAAgB;EACrBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,QAAO,GAAIL,GAAG,CAAC,IAAI;IACvB,MAAMM,KAAI,GAAIP,QAAQ,EAAE,EAAE;IAC1B,IAAIQ,cAAa,GAAIN,QAAQ,CAAC,MAAKK,KAAK,CAACE,KAAK,CAACC,SAAS,CAAC;IACzDC,OAAO,CAACC,GAAG,CAACJ,cAAc,CAA