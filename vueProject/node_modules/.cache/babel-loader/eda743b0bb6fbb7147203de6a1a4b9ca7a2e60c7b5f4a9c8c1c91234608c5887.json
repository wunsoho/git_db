onsole.log(contentList ,'저장')\r\n                set_kindList()\r\n            }\r\n\r\n            //내용 추가\r\n\r\n            //내용 추가하기 버튼 클릭시\r\n            var addInput = ref(false)\r\n            console.log(addInput.value)\r\n            function addCategory(){\r\n                addInput.value=true\r\n            }\r\n\r\n            //내용 입력후 엔터키 누를시 \r\n            var expendAddContent = ref('')\r\n            function completeAddCategory(){\r\n                if(expendAddContent.value == ''){\r\n\r\n                }\r\n                else{\r\n                    contentList.value.push({kind:expendAddContent.value, check:false})\r\n                    addexpendAddcontent.value.push({kind:expendAddContent.value, check:false})\r\n                    expendAddContent.value=''\r\n                }\r\n                \r\n                addInput.value=false\r\n            }\r\n            return{\r\n                clickOk,\r\n                closeDetailAddPopup,\r\n                contentList,\r\n                addCategory,\r\n                completeAddCategory,\r\n                addInput,\r\n                expendAddContent\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n.popup-kind-block{\r\n    border:1px solid #949494;\r\n    height:3vh;\r\n    width:inherit;\r\n    margin-top:1vh;\r\n    padding:0.2vh 2vw;\r\n    padding-left:0;\r\n}\r\n.kind-content{\r\n    font-size:1.5vh;\r\n    width:21.9vw;\r\n    height:3vh;\r\n    line-height:3vh;\r\n    color:#1f1f1f;\r\n    font-weight: 600;\r\n}\r\n.edit-category-add{\r\n    margin-top:2vh;\r\n    font-weight: 900;\r\n    color:#5F5F5F\r\n}\r\n.edit-category-add:hover{\r\n    cursor:pointer;\r\n}\r\n</style>"],"mappings":";AA8BA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAM;AACpB,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,WAAU,GAAIN,GAAG,CAAC,EAAE;IACxB,IAAIO,gBAAe,GAAIP,GAAG,CAAC,EAAE;IAC7B,IAAIQ,mBAAkB