n                info_list.Division.운영주체 = main_agent.value\r\n                info_list.Division.건물명 = document.getElementById('building_name_text').value\r\n                info_list.Division.공급처 = 공급처.value\r\n\r\n                if(main_agent.value == '기업'){\r\n                    info_list.scope = 1\r\n                }\r\n                else if(main_agent.value == \"민간\"){\r\n                    info_list.scope = 2\r\n                }\r\n\r\n                store.commit(\"SetTableContent\",info_list)\r\n            }\r\n            return {\r\n                click_regi_btn,\r\n                unit_s,\r\n                main_agent,\r\n                공급처\r\n            }\r\n        }\r\n    }\r\n</script>"],"mappings":"AAmDA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,GAAG,QAAO,KAAI;AAElB,eAAe;EACXC,IAAG,EAAG,aAAa;EACnBC,KAAKA,CAAA,EAAE;IACH,IAAIC,MAAK,GAAIH,GAAG,CAAC,IAAI;IACrB,IAAII,UAAS,GAAIJ,GAAG,CAAC,IAAI;IACzB,IAAIK,GAAE,GAAIL,GAAG,CAAC,SAAS;IACvB,MAAMM,KAAI,GAAIP,QAAQ,EAAC;IACvB,SAASQ,cAAcA,CAAA,EAAE;MACrB,IAAIC,SAAS,GAAC;QACVC,IAAI,EAAC,GAAG;QACRC,UAAU,EAAC,IAAI;QACfC,SAAS,EAAC,EAAE;QACZC,OAAO,EAAC,EAAE;QACVC,QAAQ,EAAC,EAAE;QACXC,KAAK,EAAC,CAAC;QACPC,IAAI,EAAC,EAAE;QACPC,SAAS,EAAC,EAAE;QACZC,UAAU,EAAC,IAAI;QACfC,cAAc,EAAC,EAAE;QACjBC,IAAI,EAAC,EAAE;QACPC,QAAQ,EAAC;UAACC,GAAG,EAAC,EAAE;UAACC,IAAI,EAAC,EAAE;UAACjB,GAAG,EAAC,EAAE;UAACkB,IAAI,EAAC;QAAE;MAC3C;MACA,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,mBAAmB,CAAC,CAACC,KAAI;MACnEnB,SAAS,CAACU,cAAa,GAAIO,QAAQ,CAACC,cAAc,CAAC,gCAAgC,CAAC,CAACC,KAAI;MACzFnB,SAAS,CAACO,IAAG,GAAKS,WAAW,GAAC,GAAG,GAAC,IAAG;MACrChB,SAAS,CAACQ,SAAQ,GAAIQ,WAAU,GAAI,IAAI,IAAG,EAAE;MAC7ChB,SAAS,CAACG,SAAQ,GAAIc,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtEnB,SAAS,CAACI,OAAM,GAAIa,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,KAAK,GAAC,KAAI;MAClEnB,SAAS,CAACY,QAAQ,CAACE,IAAG,GAAIlB,UAAU,CAACuB,KAAI;MACzCnB,SAAS,CAACY,QAAQ,CAACC,GAAE,GAAII,QAAQ,CAACC,cAAc,CAAC,oBAAoB,CAAC,CAACC,KAAI;MAC3EnB,SAAS,CAACY,QAAQ,CAACf,GAAE,GAAIA,GAAG,CAACsB,KAAI;MA