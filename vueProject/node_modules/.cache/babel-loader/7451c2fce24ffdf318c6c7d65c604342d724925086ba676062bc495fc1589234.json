버튼 클릭시\n            var addInput = ref(false)\n            console.log(addInput.value)\n            function addCategory(){\n                addInput.value=true\n            }\n\n            //내용 입력후 엔터키 누를시 \n            var incomeAddContent = ref('')\n            function completeAddCategory(){\n                if(incomeAddContent.value == ''){\n\n                }\n                else{\n                    contentList.push({kind:incomeAddContent.value, check:false})\n                    incomeAddContent.value=''\n                }\n                \n                addInput.value=false\n            }\n            return{\n                clickOk,\n                closeDetailAddPopup,\n                contentList,\n                addCategory,\n                completeAddCategory,\n                addInput,\n                incomeAddContent\n            }\n        }\n    }\n\n\n</script>\n\n<style>\n\n.popup-kind-block{\n    border:1px solid #949494;\n    height:3vh;\n    width:inherit;\n    margin-top:1vh;\n    padding:0.2vh 2vw;\n    padding-left:0;\n}\n.kind-content{\n    font-size:1.5vh;\n    width:21.9vw;\n    height:3vh;\n    line-height:3vh;\n    color:#1f1f1f;\n    font-weight: 600;\n}\n.edit-category-add{\n    margin-top:2vh;\n    font-weight: 900;\n    color:#5F5F5F\n}\n.edit-category-add:hover{\n    cursor:pointer;\n}\n</style>"],"mappings":";AA8BA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAASC,QAAO,QAAS,MAAM;AAE3B,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,WAAU,GAAI,CACd;MAACC,IAAI,EAAC,MAAM;MAAEC,KAAK,EAAC;IAAK,CAAC,EAC1B;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAC;IAAK,CAAC,EACxB;MAACD,IAAI,EAAC,KAAK;MAAEC,KAAK,EAAC;IAAK,CAAC,CAE7B;IACA,IAAIC,KAAI,GAAIP,QAAQ,EAAE;;IAEtB;IACA,SAASQ,mBAAmBA,CAAA,EAAE;MAC1BC,OAAO,CAACC,GAAG,CAAC,IAAI;MAChBH,KAAK,CAACI,MAAM,CAAC,mBAAmB;IACpC;;IAEA;IACA,SAASC,OAAOA,CAAA,EAAE;MACdJ,mBAAmB,EAAC;IACxB;;IAEA;;IAEA;IACA,IAAIK,QAAO,GAAId,GAAG,CAAC,KAAK;IACxBU,OAAO,CAACC,GAAG,CAACG,QAAQ,CAACC,KAAK;IAC1B,