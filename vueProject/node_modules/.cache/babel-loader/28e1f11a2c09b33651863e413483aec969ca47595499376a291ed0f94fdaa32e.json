lass=\"title add-btn\" @click=\"openAddContentPopup()\">+ 내용 추가하기</div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport {ref} from 'vue'\nimport { useStore } from 'vuex';\n\n    export default{ \n        name:'accountDetailCom',\n        components:{\n        },\n        setup(){\n            var contentList = [\n                {kind:'여가활동', check:false},\n                {kind:'간식', check:false},\n                {kind:'음식점', check:false},\n                {kind:'약', check:false},\n            ]\n            var store = useStore();\n            function closeDetailAddPopup(){\n                console.log('삭제')\n                store.commit('closeEditCategory')\n            }\n            function clickOk(){\n                \n                closeDetailAddPopup()\n            }\n            return{\n                clickOk,\n                closeDetailAddPopup,\n                contentList\n            }\n        }\n    }\n\n\n</script>\n\n<style>\n\n.popup-kind-block{\n    border:1px solid #949494;\n    height:3vh;\n    width:inherit;\n    margin-top:1vh;\n    padding:0.2vh 2vwvw\n}\n.kind-content{\n    font-size:1.5vh;\n    width:21.9vw;\n    height:3vh;\n    line-height:3vh;\n    color:#1f1f1f;\n    font-weight: 600;\n}\n</style>"],"mappings":"AA2BA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAASC,QAAO,QAAS,MAAM;AAE3B,eAAc;EACVC,IAAI,EAAC,kBAAkB;EACvBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,IAAIC,WAAU,GAAI,CACd;MAACC,IAAI,EAAC,MAAM;MAAEC,KAAK,EAAC;IAAK,CAAC,EAC1B;MAACD,IAAI,EAAC,IAAI;MAAEC,KAAK,EAAC;IAAK,CAAC,EACxB;MAACD,IAAI,EAAC,KAAK;MAAEC,KAAK,EAAC;IAAK,CAAC,EACzB;MAACD,IAAI,EAAC,GAAG;MAAEC,KAAK,EAAC;IAAK,CAAC,CAC3B;IACA,IAAIC,KAAI,GAAIP,QAAQ,EAAE;IACtB,SAASQ,mBAAmBA,CAAA,EAAE;MAC1BC,OAAO,CAACC,GAAG,CAAC,IAAI;MAChBH,KAAK,CAACI,MAAM,CAAC,mBAAmB;IACpC;IACA,SAASC,OAAOA,CAAA,EAAE;MAEdJ,mBAAmB,EAAC;IACxB;IACA,OAAM;MACFI,OAAO;MACPJ,mBAAmB;MACnBJ;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}