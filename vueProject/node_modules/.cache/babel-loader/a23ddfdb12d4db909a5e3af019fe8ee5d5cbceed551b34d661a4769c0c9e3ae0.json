{"ast":null,"code":"import { useStore } from 'vuex';\nimport { ref, computed } from 'vue';\nimport axios from \"axios\";\nexport default {\n  name: \"waste_disposal_filling\",\n  setup() {\n    const store = useStore();\n    var kindOfwaste = ref(0);\n    var selected = computed(() => store.state.selected_row);\n    var waste_life_list = ref([\"혼합 폐기물\", \"종이/편지\", \"섬유\", \"음식물\", \"목재\", \"정원 및 공원 폐기물\", \"기져귀\", \"고무 및 가죽\", \"플라스틱\", \"금속\", \"유리\", \"기타,비활성(불연성)\"]);\n    var waste_corpor_list = ref([\"혼합 폐기물\", \"음식, 음료 및 담배\", \"섬유\", \"나무 및 목제품\", \"제지\", \"석유제품, 용매, 플라스틱\", \"고무\", \"건설 및 파쇄 잔대물\", \"기타\", \"하수 슬러지\", \"폐수 슬러지\"]);\n    async function click_edit_btn() {\n      var info_list = {\n        content: \"\",\n        data: \"\",\n        emissions: \"\",\n        StartDate: \"\",\n        EndDate: \"\",\n        scope: \"1\",\n        category: \"14\"\n      };\n      var usage_input = document.getElementById('steam_usage_input').value;\n      info_list.content = document.getElementById('carbon_emissions_content_filling').value;\n      info_list.data = usage_input + \"/\" + \"ton\";\n      info_list.emissions = usage_input + 4;\n      info_list.StartDate = document.getElementById('start_data').value + '-01';\n      //info_list.EndDate = document.getElementById('end_data').value\n\n      var plz = {\n        \"CarbonData\": {\n          \"StartDate\": document.getElementById('start_data').value + '-01',\n          \"EndDate\": document.getElementById('start_data').value + '-01',\n          \"Location\": \"\",\n          \"Scope\": Number(info_list.scope),\n          \"CarbonActivity\": document.getElementById('carbon_emissions_content_filling').value,\n          \"CarbonUnit\": unit_s,\n          \"usage\": usage_input + \"/\" + unit_s,\n          \"Chief\": null\n        },\n        \"DetailType\": \"폐기물 처리시설 매립\"\n        //\"RootCom\":\"samsung\",\n        //\"BelongCom\":\"\",\n      };\n\n      var table = computed(() => store.state.table_kind);\n      console.log(\"테이블 종류\", table);\n      if (table.value == 'total_table') {\n        //수정 API 연결\n        var config = {\n          headers: {\n            \"Authorization\": \"Bearer\" + \" \" + store.state.accessToken\n          }\n        };\n        //console.log(selected.value[0])\n        console.log(plz);\n        await axios.put(\"/CarbonEmission/\" + selected.value[0].id, plz, config).then(res => {\n          console.log(plz);\n        }).catch(error => {\n          alert(\"다시 시도해주세요.\");\n          console.log(error);\n          //router.push('/');\n        }).finally(() => {\n          console.log(\"lender1\");\n        });\n      } else if (table.value == 'table') {\n        store.commit(\"SetTableContent\", info_list);\n        store.commit('DelTableContent', selected.value);\n      }\n      store.commit(\"SetEditDelet\");\n      location.reload();\n    }\n    function click_del_editPopup() {\n      console.log('수정창 닫기');\n      store.commit(\"SetEditDelet\");\n    }\n    return {\n      kindOfwaste,\n      waste_life_list,\n      waste_corpor_list,\n      click_edit_btn,\n      click_del_editPopup\n    };\n  }\n};","map":{"version":3,"names":["useStore","ref","computed","axios","name","setup","store","kindOfwaste","selected","state","selected_row","waste_life_list","waste_corpor_list","click_edit_btn","info_list","content","data","emissions","StartDate","EndDate","scope","category","usage_input","document","getElementById","value","plz","Number","unit_s","table","table_kind","console","log","config","headers","accessToken","put","id","then","res","catch","error","alert","finally","commit","location","reload","click_del_editPopup"],"sources":["/home/hyemi/carbon/co2-emission-management/vueProject/src/components/measure/EditDivide-carbon/EditWaste_disposal(filling).vue"],"sourcesContent":["<!-- 폐기물 처리 시설(매립)  -->\n<template>\n    <div>\n        <div class=\"edit_top \">\n            <span class=\"edit_category\">폐기물 처리시설(매립) 정보 추가하기</span>\n            <button class=\"del_edit_btn\" @click=\"click_del_editPopup()\">x</button>\n        </div>\n        <div style=\"background:#ffffff; padding:3vh; width:25vw; height:70vh; font-size:1.8vh\">\n            <div>\n                <div style=\"color:#000000;\">\n                탄소 배출 내용<br>   \n                <textarea class=\"addInfo_input\" id=\"carbon_emissions_content_filling\" rows=\"8\" style=\"height:12vh; width:25vw\"></textarea>\n                </div> \n                <div class=\"add_info_divide\" id=\"building_name_text\" style=\"margin-top:4vh; \">시설명/위치\n                <input type=\"text\" class=\"addInfo_input\" style=\"margin-left:94px; width:11.5vw; height:3.5vh\" id =\"building_name_input\" placeholder=\"경상대 본관\">\n            </div>\n            <div style=\"margin-top:30px; font-size:1.4vh\">매립 시작 날짜\n                <input class = \"date_btn\" id = \"start_data\" type=\"month\" data-placeholder=\"시작 날짜\" required aria-required=\"true\">\n            </div>\n\n            <div class=\"add_info_divide\">폐기물 분류\n                <select v-model=\"kindOfwaste\" class=\"addInfo_input\" id=\"operating_entity_input\" style=\"width:11.5vw;, height:3.5vh\">\n                    <option :value=\"0\">생활 폐기물</option>\n                    <option :value=\"1\">사업장 폐기물</option>\n                </select>\n            </div>\n            <div class=\"add_info_divide\">폐기물 세부 구분\n                <select class=\"addInfo_input\" id=\"supplier_drop\" v-if=\"kindOfwaste == '0'\" style=\"margin-left:61px; width:11.5vw\">\n                    <option v-for = \"waste in waste_life_list\" >{{waste}}</option>\n                </select>\n                <select class=\"addInfo_input\" id=\"supplier_drop\" v-else-if=\"kindOfwaste == '1'\" style=\"margin-left:61px; width:11.5vw\">\n                    <option v-for = \"waste in waste_corpor_list\" >{{waste}}</option>\n                </select>\n            </div>\n            <div class=\"add_info_divide\">매립 시설 유형\n                <select class=\"addInfo_input\" id=\"supplier_drop_steam\" style=\"margin-left:73px; width:11.5vw\">\n                    <option value=\"0\">관리형 매립지 - 혐기성</option>\n                    <option value=\"1\">관리형 매립지 - 준호기성</option>\n                    <option value=\"2\">비관리형 매립지 - 매립고 5cm 이상</option>\n                    <option value=\"3\">비관리형 매립지 - 매립고 5cm 미만</option>\n                    <option value=\"4\">기타</option>\n                </select>\n            </div>\n            <div class=\"add_info_divide\" >매립 양\n                <input class=\"addInfo_input\" id=\"steam_usage_input\" placeholder=\"12,456\" style=\"margin-left:120px; height:3.5vh; width:8.5vw\">\n                <select class=\"addInfo_input\" id=\"steam_usage_drop\" style=\"height:3.5vh; width:2.2vw\">\n                    <option value=\"0\">ton</option>\n                </select>\n            </div>\n            </div>\n        </div>\n        <div class=\"edit_bottom\" style=\"font-size:1.5vh\">\n            <button class=\"edit_regi_btn\" id=\"edit_regi\" @click=\"click_edit_btn()\">수정하기</button>\n            <button class=\"edit_regi_btn\" id=\"edit_cancle\" @click=\"click_del_editPopup()\">취소</button>\n        </div>\n    </div>\n\n</template>\n\n<style>\n\n    #supplier_drop_steam{\n        width: 20%;\n        margin-left: 90px;\n        color: #727374\n    }\n\n    #steam_usage_input{\n        margin-left:90px;\n        width:14%;\n        background: #ffffff;\n        border: 1px solid #DDE2E5;\n    }\n\n    #steam_usage_drop{\n        width: 4.6%;\n        margin-left: 1%;\n        color: #727374;\n        margin-bottom: 20px;\n    }\n</style>\n\n<script>\nimport {useStore} from 'vuex'\nimport {ref, computed} from 'vue'\nimport axios from \"axios\";\n    export default {\n        name :\"waste_disposal_filling\",\n        setup(){\n            const store = useStore()\n            var kindOfwaste =  ref(0)\n            var selected = computed(()=>store.state.selected_row);\n            var waste_life_list = ref([\n                    \"혼합 폐기물\",\n                    \"종이/편지\",\n                    \"섬유\",\n                    \"음식물\",\n                    \"목재\",\n                    \"정원 및 공원 폐기물\",\n                    \"기져귀\",\n                    \"고무 및 가죽\",\n                    \"플라스틱\",\n                    \"금속\",\n                    \"유리\",\n                    \"기타,비활성(불연성)\"\n                ])\n            var waste_corpor_list = ref([\n                \"혼합 폐기물\",\n                \"음식, 음료 및 담배\",\n                \"섬유\",\n                \"나무 및 목제품\",\n                \"제지\",\n                \"석유제품, 용매, 플라스틱\",\n                \"고무\",\n                \"건설 및 파쇄 잔대물\",\n                \"기타\",\n                \"하수 슬러지\",\n                \"폐수 슬러지\"\n            ])\n            async function click_edit_btn(){\n                var  info_list = {content:\"\",data:\"\",emissions:\"\",StartDate:\"\",EndDate:\"\",scope:\"1\",category:\"14\"}\n                var usage_input = document.getElementById('steam_usage_input').value\n\n                info_list.content = document.getElementById('carbon_emissions_content_filling').value\n                info_list.data =  usage_input+\"/\"+\"ton\"\n                info_list.emissions = usage_input+4\n                info_list.StartDate = document.getElementById('start_data').value+'-01'\n                //info_list.EndDate = document.getElementById('end_data').value\n                \n                var plz = {\n                        \"CarbonData\": {\n                            \"StartDate\":document.getElementById('start_data').value+'-01',\n                            \"EndDate\":document.getElementById('start_data').value+'-01',\n                            \"Location\": \"\",\n                            \"Scope\":  Number(info_list.scope),\n                            \"CarbonActivity\": document.getElementById('carbon_emissions_content_filling').value,\n                            \"CarbonUnit\": unit_s,\n                            \"usage\": usage_input+\"/\"+unit_s,\n                            \"Chief\": null\n                        },\n                        \"DetailType\":\"폐기물 처리시설 매립\",\n                        //\"RootCom\":\"samsung\",\n                        //\"BelongCom\":\"\",\n                }\n                var table = computed(() => store.state.table_kind)\n                console.log(\"테이블 종류\",table)\n                if(table.value == 'total_table'){\n                    //수정 API 연결\n                    var config = {\n                        headers:{\n                            \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\n                        }\n                    }\n                   //console.log(selected.value[0])\n                   console.log(plz)\n                    await axios.put(\"/CarbonEmission/\"+selected.value[0].id,plz,config).then(res => {\n                        console.log(plz)\n                        \n                    })\n                    .catch(error => {\n                        alert(\"다시 시도해주세요.\")\n                        console.log(error)\n                        //router.push('/');\n                    })\n                    .finally(() => {\n                        console.log(\"lender1\")\n                    })\n                }\n                else if(table.value == 'table'){\n                    store.commit(\"SetTableContent\",info_list);\n                    store.commit('DelTableContent',selected.value);\n                }\n                store.commit(\"SetEditDelet\");\n                location.reload();\n            }\n            \n            function click_del_editPopup(){\n                console.log('수정창 닫기')\n                store.commit(\"SetEditDelet\");\n            }\n            return {\n                kindOfwaste,\n                waste_life_list,\n                waste_corpor_list,\n                click_edit_btn,\n                click_del_editPopup\n            }\n        }\n    }\n</script>"],"mappings":"AAmFA,SAAQA,QAAQ,QAAO,MAAK;AAC5B,SAAQC,GAAG,EAAEC,QAAQ,QAAO,KAAI;AAChC,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,wBAAwB;EAC9BC,KAAKA,CAAA,EAAE;IACH,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,WAAU,GAAKN,GAAG,CAAC,CAAC;IACxB,IAAIO,QAAO,GAAIN,QAAQ,CAAC,MAAII,KAAK,CAACG,KAAK,CAACC,YAAY,CAAC;IACrD,IAAIC,eAAc,GAAIV,GAAG,CAAC,CAClB,QAAQ,EACR,OAAO,EACP,IAAI,EACJ,KAAK,EACL,IAAI,EACJ,aAAa,EACb,KAAK,EACL,SAAS,EACT,MAAM,EACN,IAAI,EACJ,IAAI,EACJ,aAAY,CACf;IACL,IAAIW,iBAAgB,GAAIX,GAAG,CAAC,CACxB,QAAQ,EACR,aAAa,EACb,IAAI,EACJ,UAAU,EACV,IAAI,EACJ,gBAAgB,EAChB,IAAI,EACJ,aAAa,EACb,IAAI,EACJ,QAAQ,EACR,QAAO,CACV;IACD,eAAeY,cAAcA,CAAA,EAAE;MAC3B,IAAKC,SAAQ,GAAI;QAACC,OAAO,EAAC,EAAE;QAACC,IAAI,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,SAAS,EAAC,EAAE;QAACC,OAAO,EAAC,EAAE;QAACC,KAAK,EAAC,GAAG;QAACC,QAAQ,EAAC;MAAI;MACjG,IAAIC,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAAC,mBAAmB,CAAC,CAACC,KAAI;MAEnEX,SAAS,CAACC,OAAM,GAAIQ,QAAQ,CAACC,cAAc,CAAC,kCAAkC,CAAC,CAACC,KAAI;MACpFX,SAAS,CAACE,IAAG,GAAKM,WAAW,GAAC,GAAG,GAAC,KAAI;MACtCR,SAAS,CAACG,SAAQ,GAAIK,WAAW,GAAC;MAClCR,SAAS,CAACI,SAAQ,GAAIK,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAI;MACtE;;MAEA,IAAIC,GAAE,GAAI;QACF,YAAY,EAAE;UACV,WAAW,EAACH,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC7D,SAAS,EAACF,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,CAACC,KAAK,GAAC,KAAK;UAC3D,UAAU,EAAE,EAAE;UACd,OAAO,EAAGE,MAAM,CAACb,SAAS,CAACM,KAAK,CAAC;UACjC,gBAAgB,EAAEG,QAAQ,CAACC,cAAc,CAAC,kCAAkC,CAAC,CAACC,KAAK;UACnF,YAAY,EAAEG,MAAM;UACpB,OAAO,EAAEN,WAAW,GAAC,GAAG,GAACM,MAAM;UAC/B,OAAO,EAAE;QACb,CAAC;QACD,YAAY,EAAC;QACb;QACA;MACR;;MACA,IAAIC,KAAI,GAAI3B,QAAQ,CAAC,MAAMI,KAAK,CAACG,KAAK,CAACqB,UAAU;MACjDC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAACH,KAAK;MAC1B,IAAGA,KAAK,CAACJ,KAAI,IAAK,aAAa,EAAC;QAC5B;QACA,IAAIQ,MAAK,GAAI;UACTC,OAAO,EAAC;YACJ,eAAe,EAAC,QAAQ,GAAC,GAAG,GAAC5B,KAAK,CAACG,KAAK,CAAC0B;UAC7C;QACJ;QACD;QACAJ,OAAO,CAACC,GAAG,CAACN,GAAG;QACd,MAAMvB,KAAK,CAACiC,GAAG,CAAC,kBAAkB,GAAC5B,QAAQ,C