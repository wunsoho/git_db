ver(group_list){\n                var config = {\n                    headers:{\n                    \"Authorization\":\"Bearer\"+\" \"+store.state.accessToken\n                    }\n                }\n                console.log(\"추가하기\",group_list)\n                await axios.post(\"Company/PreviewInfo/\"+group_name.value, group_list, config).then(res => {\n                    console.log(res.data)\n                })\n                .catch(error => {\n                    alert(\"로그인 시간이 만료되었습니다.\")\n                    console.log(error)\n                    router.push('/');\n                })\n                .finally(() => {})\n                location.reload();\n            }\n            return {GroupAddBtn,offpopup,group_name,addGroup,depth,add_group_server}\n        },\n        mounted(){\n        },\n        destroyed(){\n            add_group_server\n        }\n    };\n</script>"],"mappings":";AA4EA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,UAAU,EAACC,SAAQ,QAAS,YAAY;AACjD,OAAOC,KAAI,MAAO,OAAO;AACrB,eAAe;EACXC,IAAG,EAAG,UAAU;EAChBC,UAAU,EAAC,CACX,CAAC;EACDC,KAAKA,CAAA,EAAE;IACH,MAAMC,MAAK,GAAIL,SAAS,EAAE;IAC1B,MAAMM,KAAI,GAAIR,QAAQ,EAAE,EAAE;IAC1B,IAAIS,WAAU,GAAIV,QAAQ,CAAC,MAAKS,KAAK,CAACE,KAAK,CAACD,WAAW,CAAC;IACxD,IAAIE,UAAS,GAAIZ,QAAQ,CAAC,MAAKS,KAAK,CAACE,KAAK,CAACC,UAAU,CAAC;IACtD,IAAIC,KAAI,GAAIb,QAAQ,CAAC,MAAKS,KAAK,CAACE,KAAK,CAACE,KAAK,CAAC;IAC5C,SAASC,QAAQA,CAAA,EAAE;MACfL,KAAK,CAACM,MAAM,CAAC,kBAAkB,CAAC;IACpC;IAEA,SAASC,QAAQA,CAAA,EAAE;MACf,IAAIC,UAAU,GAAC;QACXC,OAAO,EAACC,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC,CAACC,KAAK;QACxDC,MAAM,EAAC,CAAC;QACRC,MAAM,EAAC,CAAC;QACRC,MAAM,EAAC,CAAC;QACRC,KAAK,EAACN,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC,CAACC,KAAK;QACtDK,KAAK,EAAC,EAAE;QACRC,cAAc,EAAC,EAAE;QACjBC,WAAW,EAAC,EAAE;QACdC,QAAQ,EAAC,EAAE;QACXC,cAAc,EAACX,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC,CAACC,KAAK;QAC/DU,KAAK,EAACC,MAAM,CAACnB,KAAK,CAACQ,KAAK;MAC5B;MACAY,OAAO,CAACC,GAAG,CAAC,MAAM,EAACjB,UAAU;MAC7BkB,gBAAgB,CAAClB,UAAU;MAC3B;MACAR,KAAK,